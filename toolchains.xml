<!-- Copy this file to ~/.m2/toolchains.xml and adjust according to your JRE locations to
     give your Maven / Tycho installation knowledge of how to resolve the different
     execution environments.
-->
<toolchains>
  <!-- JDK toolchains -->
  <toolchain>
      <type>jdk</type>
      <provides>
            <version>1.8</version>
            <id>JavaSE-1.8</id>
            <vendor>sun</vendor>
      </provides>
      <configuration>
            <jdkHome>/opt/sapjvm_8/jre</jdkHome>
      </configuration>
  </toolchain>
  <toolchain>
      <type>jdk</type>
      <provides>
            <version>11</version>
            <id>JavaSE-11</id>
            <vendor>sun</vendor>
      </provides>
      <configuration>
            <jdkHome>/usr/lib/jvm/java-11-openjdk-amd64</jdkHome>
      </configuration>
  </toolchain>
  <toolchain>
      <type>jdk</type>
      <provides>
            <version>17</version>
            <id>JavaSE-17</id>
            <vendor>sun</vendor>
      </provides>
      <configuration>
            <jdkHome>/usr/lib/jvm/sapmachine-jdk-17.0.10</jdkHome>
      </configuration>
  </toolchain>
</toolchains>
