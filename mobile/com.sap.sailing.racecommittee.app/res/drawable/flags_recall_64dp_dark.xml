<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:left="40dp"
        android:top="30dp">
        <bitmap
            android:gravity="top"
            android:src="@drawable/flag_xray_64dp"/>
    </item>

    <item
        android:left="-30dp"
        android:top="0dp">
        <bitmap
            android:gravity="center"
            android:src="@drawable/flag_shape_04_64dp_dark"/>
    </item>

    <item
        android:left="-40dp"
        android:top="-30dp">
        <bitmap
            android:gravity="center"
            android:src="@drawable/flag_first_substitute_64dp"/>
    </item>

</layer-list>