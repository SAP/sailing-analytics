<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >
    
    <ImageView 
        android:id="@+id/compass_view_background"
        android:src="@drawable/compass_back_302dpi_new"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/compass_view_compass"
        android:layout_gravity="center"
        android:layout_centerInParent="true" />
    
    <ImageView 
        android:id="@+id/compass_view_needle"
        android:src="@drawable/compass_needle_302dpi_north_new"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/compass_view_needle"
        android:layout_gravity="center"
        android:layout_centerInParent="true" />

</merge>