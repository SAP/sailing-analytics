<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
	<worksheet name="Erg_Drachen">
		<section startRow="0" endRow="11">
			<mapping cell="A2">regattaInfo.eventName</mapping>
			<mapping cell="A3">regattaInfo.boatClass</mapping>
			<mapping cell="A4">regattaInfo.date</mapping>
		</section>
		<loop startRow="12" endRow="12" items="competitors" var="competitor"
			varType="com.sap.sailing.winregatta.resultimport.impl.CompetitorRowImpl">
			<section startRow="12" endRow="12">
				<mapping row="12" col="0">competitor.totalRank</mapping>
				<mapping row="12" col="1">competitor.country</mapping>
				<mapping row="12" col="2">competitor.sailNumber</mapping>
				<mapping row="12" col="3">competitor.teamMember1Name</mapping>
				<mapping row="12" col="4">competitor.teamMember1Club</mapping>
				<mapping row="12" col="5">competitor.teamMember1DSVNumber</mapping>
				<mapping row="12" col="6">competitor.teamMember2Name</mapping>
				<mapping row="12" col="7">competitor.teamMember2Club</mapping>
				<mapping row="12" col="8">competitor.teamMember2DSVNumber</mapping>
				<mapping row="12" col="9">competitor.teamMember3Name</mapping>
				<mapping row="12" col="10">competitor.teamMember3Club</mapping>
				<mapping row="12" col="11">competitor.teamMember3DSVNumber</mapping>
				<mapping row="12" col="12">competitor.race1Score</mapping>
				<mapping row="12" col="13">competitor.race2Score</mapping>
				<mapping row="12" col="14">competitor.race3Score</mapping>
				<mapping row="12" col="15">competitor.race4Score</mapping>
				<mapping row="12" col="16">competitor.race5Score</mapping>
				<mapping row="12" col="17">competitor.race6Score</mapping>
				<mapping row="12" col="18">competitor.totalScore</mapping>
			</section>
			<loopbreakcondition>
			<!-- stops at first cell with no te in the first column -->
				<rowcheck offset="0">
					<cellcheck offset="0">END</cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>
