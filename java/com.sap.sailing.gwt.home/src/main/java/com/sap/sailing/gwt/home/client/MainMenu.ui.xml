<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="com.sap.sailing.gwt.home.client.PageNameTokens" field="pageNames"></ui:with>

    <g:HTMLPanel>
    	<g:InlineHyperlink targetHistoryToken="{pageNames.getStartPage}">Home</g:InlineHyperlink> |
        <g:InlineHyperlink targetHistoryToken="{pageNames.getEventsPage}">Events</g:InlineHyperlink> |
        <g:InlineHyperlink targetHistoryToken="{pageNames.getAboutUsPage}">About Us</g:InlineHyperlink> |
        <g:InlineHyperlink targetHistoryToken="{pageNames.getContactPage}">Contact</g:InlineHyperlink>
	</g:HTMLPanel>
</ui:UiBinder>
