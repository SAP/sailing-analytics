<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
	<ui:with field='i18n' type='com.sap.sailing.gwt.home.client.i18n.TextMessages'/>
	<ui:with field="res" type="com.sap.sailing.gwt.home.client.HomeResources" />
		<ui:style>
.siteheader {
  font-size: 16px;
  font-size: 1rem;
  background: #333;
  border-bottom: 5px solid #f0ab00;
  height: 4.0625em;
  line-height: 3.75em;
  width: 100%;
}
.logo {
  margin: 0.625em 0;
  float: left;
}
.sitenavigation {
  float: right;
}
.sitenavigation_link {
  display: inline-block;
  font-size: 0.875em;
  font-weight: 600;
  color: #f6f9fc;
  text-transform: uppercase;
  padding: 0 2.1875em;
}
.sitenavigation_link:hover {
  background-color: rgba(246,249,252,0.1);
  text-decoration: none;
}
.sitenavigation_linksearch img {
  vertical-align: middle;
}
.sitenavigationhome .sitenavigation_linkhome,
.sitenavigationevents .sitenavigation_linkevents,
.sitenavigationboatclasses .sitenavigation_linkboatclasses,
.sitenavigationsolutions .sitenavigation_linksolutions,
.sitenavigationsponsoring .sitenavigation_linksponsoring {
  background: #f0ab00;
}
.sitenavigationhome .sitenavigation_linkhome:hover,
.sitenavigationevents .sitenavigation_linkevents:hover,
.sitenavigationboatclasses .sitenavigation_linkboatclasses:hover,
.sitenavigationsolutions .sitenavigation_linksolutions:hover,
.sitenavigationsponsoring .sitenavigation_linksponsoring:hover {
  background-color: rgba(240,171,0,0.9);
}
    </ui:style>
	<g:HTML>
<header class="{style.siteheader}">
  <div class="{res.mainCss.grid}">
    <div class="{res.mainCss.small4} {res.mainCss.columns}"><img src="images/logo.png" alt="SAP Sailing Analytics" class="{style.logo}"></img></div>
    <div class="{res.mainCss.small8} {res.mainCss.columns}">
      <nav class="{style.sitenavigation}"><a href="index.html" class="{style.sitenavigation_link} {style.sitenavigation_linkhome}">Home</a><a href="events.html" class="{style.sitenavigation_link} {style.sitenavigation_linkevents}">Events</a><a href="boat-classes.html" class="{style.sitenavigation_link} {style.sitenavigation_linkboatclasses}">Boat Classes</a><a href="solutions.html" class="{style.sitenavigation_link} {style.sitenavigation_linksolutions}">Solutions</a><a href="sponsoring.html" class="{style.sitenavigation_link} {style.sitenavigation_linksponsoring}">Sponsoring</a><a href="" class="{style.sitenavigation_link} {style.sitenavigation_linksearch}"><img src="images/search-icon.png" alt="Search"></img></a></nav>
    </div>
  </div>
</header>
	</g:HTML>

</ui:UiBinder>