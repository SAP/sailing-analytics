<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'>
	<ui:style>
        @external slick-slide, slick-prev, slick-next, slick-track;
        .slider {
			width: 100%;

        }

		.slider .slick-track {
			overflow-y:hidden;
		}
		.slider  li {
			width: 10px;
			padding: 2px;
		}

		.slider .slick-dots li button:before {
			width: 10px;
		}

		.slider ul {
			position: absolute;
			bottom: -90px;
			font-size: 10px;
			overflow: hidden;
			height: 70px;
		}

		.slider .slick-dots li {
			margin: 5px;
			width: 10px;
		}

        .slider ul > li > button:before {
            font-size: 10px;
			width: 5spx;
			padding: 0px;
        }
		

		.slider .slick-slide {
            margin-left: 15px;
            margin-right: 15px;
			background-color: #fcfcfc;
		}
		
		.slider .slick-prev {
			left: 20px;
			z-index:1;
			height: 60px;
			width: 60px;

		}
		.slider .slick-prev:before {
			color: #000000;
			font-size: 60px;
			left: -60px;
			
		} 
		.slider .slick-next {
			right: 20px;
			z-index:1;
			height: 60px;
			width: 60px;
		} 
		.slider .slick-next:before {
			color: #000000;
			font-size: 60px;
			left: -60px;
		} 
    </ui:style>
    
	<div class="{style.slider}" ui:field="sliderMainUi" >
	</div>
</ui:UiBinder>