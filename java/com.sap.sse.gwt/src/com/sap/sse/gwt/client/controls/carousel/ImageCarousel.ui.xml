<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'>
	<ui:style>
	@external slick-slide, slick-prev, slick-next, slick-track, slick-dots,slick-active;
	.slider {
		width: 100%;
	}
	
	.slider .slick-track {
		overflow-y: hidden;
	}
	
	.slider  li {
		width: 10px;
		padding: 2px;
	}
	
	.slider .slick-dots {
		position: absolute;
		bottom: -45px;
		display: inline-block;
		width: 100%;
		padding: 0;
		list-style: none;
		text-align: center;
	}
	
	.slider .slick-dots>li {
		position: relative;
		display: inline-block;
		width: 20px;
		height: 20px;
		margin: 0 5px;
		padding: 0;
		cursor: pointer;
	}
	
	.slider .slick-dots>li>button {
		font-size: 0;
		line-height: 0;
		display: block;
		width: 20px;
		height: 20px;
		padding: 5px;
		cursor: pointer;
		color: transparent;
		border: 0;
		outline: none;
		background: transparent;
	}
	
	.slider 	.slick-dots>li>button:hover, .slider .slick-dots>li>button:focus
	{
		outline: none;
	}
	
	.slider .slick-dots>li>button:hover:before, .slider .slick-dots>li>button:focus:before
		{
		opacity: 1;
	}
	
	.slider .slick-dots>li>button:before {
		width: 10px;
	}
	
	.slider .slick-dots>li>button:before {
		font-family: 'slick';
		font-size: 35px;
		line-height: 35px;
		position: absolute;
		top: 0;
		left: 0;
		width: 20px;
		height: 20px;
		content: "\2022";
		text-align: center;
		opacity: .25;
		color: black;
		-webkit-font-smoothing: antialiased;
		-moz-osx-font-smoothing: grayscale;
	}
	
	.slider .slick-dots>li.slick-active>button:before {
		opacity: .75;
		color: black;
	}
	
	.slider .slick-slide {
		margin-left: 15px;
		margin-right: 15px;
		background-color: #fcfcfc;
	}
	
	
	.slider .slick-prev, .slider .slick-next {
		position: absolute;
		background-position: 0 0;
		background-repeat: no-repeat;
		cursor: pointer;
		top: 50%;
		-webkit-transform: translate(0, -50%);
		-moz-transform: translate(0, -50%);
		-ms-transform: translate(0, -50%);
		transform: translate(0, -50%);
		width: 4em;
		height: 4em;
		background-image:
			url(data:image/png;base64,)
	}
	
	.slider slick-prev:hover, .slider slick-prev:focus, .slider slick-next:hover,
		.slick-next:focus {
		color: transparent;
		outline: none;
	}
	
	
	.slider .slick-prev {
		left: 0px;
		height: 60px;
		width: 60px;
		background-position: 0 0;
	}

	.slider .slick-prev {
		left: 0;
		background-position: 0 0;
	}
	
	.slider .slick-prev:hover {
		background-position: 0 100%
	}
	
	.slider .slick-next {
		right: 0;
		background-position: 100% 0;
	}
	
	.slider .slick-next:hover {
		background-position: 100% 100%;
	}	
	
	</ui:style>

	<div class="{style.slider}" ui:field="sliderMainUi">
	</div>
</ui:UiBinder>