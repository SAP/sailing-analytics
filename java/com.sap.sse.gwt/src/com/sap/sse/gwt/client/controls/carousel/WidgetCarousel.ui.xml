<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

 	    @external slick-slide, slick-prev, slick-next, slick-track;

        .slider {
			width: 100%;

        }

		.slick-track {
			
		}

        .slider > ul > li > button:before {
            font-size: 15px;
        }
		
		.slick-slide {
           	margin: 0px;
			background-color: #fcfcfc;
		}

		
		.slick-prev {
			left: 20px;
			z-index:1;
			height: 60px;
			width: 60px;

		}
		.slick-prev:before {
			color: #000000;
			font-size: 60px;
			left: -60px;
		} 
		.slick-next {
			right: 20px;
			z-index:1;
			height: 60px;
			width: 60px;
		} 
		.slick-next:before {
			color: #000000;
			font-size: 60px;
			left: -60px;
		} 


    </ui:style>

	<g:HTMLPanel styleName="{style.slider}" ui:field="sliderMainUi">

	
	</g:HTMLPanel>

</ui:UiBinder>