<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module>
	<!-- Inherit the core Web Toolkit stuff.  -->
	<inherits name='com.google.gwt.user.User'/>

	<!-- Other module inherits -->
	<!-- 
	<inherits name="com.sap.sse.gwt.EmulatedJRE" />
	<inherits name="com.sap.sse.gwt.SSESharedGWT" />
	 -->
	<inherits name="com.sap.sse.SSECommon" />
<!-- 
	<super-source path="jre" />
 --> 

	<entry-point class='com.sap.sse.gwt.theme.client.ThemeEntryPoint'/>

	<set-configuration-property name="CssResource.enableGss" value="true" />
	<set-configuration-property name="CssResource.legacy" value="true" />

	<!-- Specify the paths for translatable code   -->
	<source path='client'/>
	<source path='shared'/>
	
	<!-- allow Super Dev Mode -->
	<add-linker name="xsiframe"/>
</module>
