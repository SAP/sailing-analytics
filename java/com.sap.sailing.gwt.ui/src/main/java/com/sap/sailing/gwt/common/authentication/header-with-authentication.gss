.header_right_wrapper {
    height: 100%;
}
.with_login>.header_right_extension {
	margin-right: 4em;
}
.usermanagement_icon {
	display: none;
}
.with_login>.usermanagement_icon {
    position: absolute;
    right: 0;
    display: inline-block;
    width: 4rem;
    height: 100%;
    vertical-align: middle;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: contain;
    background-image: url('images/home/loggedout.svg');
    cursor: pointer;
}
.usermanagement_icon.usermanagement_open {
	background-image: url('images/home/loggedout-open.svg');
}
.usermanagement_icon.usermanagement_loggedin {
	background-image: url('images/home/loggedin.svg');
}
.usermanagement_icon.usermanagement_loggedin.usermanagement_open {
	background-image: url('images/home/loggedin-open.svg');
}
.usermanagement_view {
	top: 70px !important;
    left: inherit !important;
    right: 0 !important;
    z-index: 10;
    line-height: 1.333;
}
.usermanagement_view * {
    font-size: 15px;
}
