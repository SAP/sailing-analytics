<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
    <ui:with field="i18n" type="com.sap.sailing.gwt.common.client.i18n.TextMessages"/>
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.place.event.overview.EventOverviewResources" />
    <g:HTMLPanel>

<!-- The overview page of an event-->
			
<!-- Ubilabs has to cut this page into partials 
<div class="{local_res.css.eventoverview}">
  <div class="{res.mediaCss.grid}">
    <div class="{res.mediaCss.small8} {res.mediaCss.columns}">
      <div class="{local_res.css.eventlivebox}">
        <figure class="{local_res.css.eventlivebox_video} {local_res.css.videowrapper}">
          <div style="background-image: url({event.livebox.video.preview});" data-src="{event.livebox.video.src}" class="{local_res.css.eventlivebox_video_container} {local_res.css.playablevideo}"><span class="{local_res.css.playablevideo_buttonplay}"></span>
            <div class="{local_res.css.eventlivebox_video_container_captionlayer}"></div>
            <figcaption class="{local_res.css.eventlivebox_video_container_caption}">{#if event.livebox.isLive}
              <div data-labeltype="live" class="{res.mainCss.label}">Live</div>{/if event.livebox.isLive}{event.livebox.caption}
            </figcaption>
          </div>
        </figure>
        <div class="{local_res.css.eventlivebox_linklist}"><a href="{link.url}" class="{local_res.css.eventlivebox_linklist_link}"><span data-type="{link.icon}" class="{local_res.css.eventlivebox_linklist_link_icon}"></span><strong class="{local_res.css.eventlivebox_linklist_link_title}">{link.title}{#if link.isLive}
              <div data-labeltype="live" class="{res.mainCss.label}">Live</div>{/if link.isLive}</strong>
            <div class="{local_res.css.eventlivebox_linklist_link_description}">{link.description}
            </div></a>
        </div>
      </div>
      <div class="{local_res.css.latestraceresults}">
        <h3 class="{local_res.css.latestraceresults_headline}">Latest Official Race Results
          <button class="{local_res.css.latestraceresults_refresh} {res.mainCss.button} {res.mainCss.buttonrefresh}">refresh</button>
        </h3>{#if result.details}
        <div class="{local_res.css.latestraceresults_detail}">
          <div class="{res.mediaCss.grid}">
            <div class="{res.mediaCss.small3} {res.mediaCss.columns} {local_res.css.latestraceresults_detail_title}">{result.title}
            </div>
            <div class="{res.mediaCss.small3} {res.mediaCss.columns} {local_res.css.latestraceresults_detail_raceCount}">Result after&nbsp;{result.totalRaces}&nbsp;races
            </div>
            <div class="{res.mediaCss.small6} {res.mediaCss.columns}">{#if result.isNew}
              <div data-labeltype="new" class="{res.mainCss.label}">New</div>{else}{/if result.isNew}{#if result.isUpdated}
              <div data-labeltype="updated" class="{res.mainCss.label}">Updated</div>{else}{/if result.isUpdated}
              <div class="{local_res.css.latestraceresults_detail_datetime}">{result.time}
              </div>
              <div class="{local_res.css.latestraceresults_detail_datetime}">Last update:&nbsp;{result.date}
              </div>
            </div>
          </div>
          <div class="{res.mediaCss.grid} {local_res.css.latestraceresults_detail_wrapper}">
            <div class="{res.mediaCss.small3} {res.mediaCss.columns}"><img src="{result.details.thumb.url}" alt="{result.details.thumb.altText}" class="{local_res.css.latestraceresults_detail_wrapper_thumb}"></img></div>
            <div class="{res.mediaCss.small9} {res.mediaCss.columns}">
              <div data-rank="{competitor.rank}" class="{local_res.css.leaderboardcompetitor} {local_res.css.leaderboardcompetitorshort}">
                <div class="{res.mediaCss.grid}">
                  <div class="{res.mediaCss.small1} {res.mediaCss.columns} {local_res.css.leaderboardcompetitor_itemcenter}">
                    <div class="{local_res.css.leaderboardcompetitor_rank}">{competitor.rank}</div>
                  </div>
                  <div class="{res.mediaCss.small1} {res.mediaCss.columns}"><img src="assets/flags/{competitor.countryCode}.png" alt="{competitor.countryCode}" width="32" height="32" class="{local_res.css.latestraceresults_detail_wrapperimage} {local_res.css.leaderboardcompetitor_nationality}"></img></div>
                  <div class="{res.mediaCss.small2} {res.mediaCss.columns}">{competitor.sailID}</div>
                  <div class="{res.mediaCss.small7} {res.mediaCss.columns}">{competitor.displayName}</div>
                  <div class="{res.mediaCss.small1} {res.mediaCss.columns} {local_res.css.leaderboardcompetitor_itemcenter}">{competitor.totalPoints}</div>
                </div>
              </div>
              <div class="{local_res.css.latestraceresults_detail_wrapper_more}"><a href="{result.details.moreLink}" class="{res.mainCss.button}">Â Leaderboard</a></div>
            </div>
          </div>
        </div>{else}<a href="{result.link}" class="{local_res.css.latestraceresults_link}"><span class="{res.mediaCss.grid}"><span class="{res.mediaCss.small3} {res.mediaCss.columns} {local_res.css.latestraceresults_link_title}">{result.title}</span><span class="{res.mediaCss.small3} {res.mediaCss.columns} {local_res.css.latestraceresults_link_raceCount}">Result after&nbsp;{result.totalRaces}&nbsp;races</span><span class="{res.mediaCss.small6} {res.mediaCss.columns}"><span data-labeltype="new" class="{res.mainCss.label}">New</span><span data-labeltype="updated" class="{res.mainCss.label}">Updated</span><span class="{local_res.css.latestraceresults_link_datetime}">{result.time}</span><span class="{local_res.css.latestraceresults_link_datetime}">{result.date}</span></span></span></a>{/if result.details}
      </div>
      <div class="{local_res.css.mediateaser} {local_res.css.mediateaserphotos}">
        <h3 class="{local_res.css.mediateaser_title}"><a href="event-media.html" class="{local_res.css.mediateaser_title_link}">Photos&nbsp;<span class="{local_res.css.mediateaser_title_value}">({event.photosCount})</span></a></h3>
        <div class="{res.mediaCss.grid} {local_res.css.lightbox}">
          <div class="{local_res.css.lightbox_data}">{event.photos[0].thumb},{event.photos[0].file},{event.photos[0].title};{event.photos[1].thumb},{event.photos[1].file},{event.photos[1].title};{event.photos[2].thumb},{event.photos[2].file},{event.photos[2].title};
          </div>
          <div class="{res.mediaCss.small9} {res.mediaCss.columns}">
            <div title="{event.photos[0].title}" data-src="{event.photos[0].file}" style="background-image: url(images/{event.photos[0].thumb});" class="{local_res.css.lightbox_photo} {local_res.css.mediateaser_photo} {local_res.css.mediateaser_photomain}"></div>
          </div>
          <div class="{res.mediaCss.small3} {res.mediaCss.columns}">
            <div title="{event.photos[1].title}" data-src="{event.photos[1].file}" style="background-image: url(images/{event.photos[1].thumb});" class="{local_res.css.lightbox_photo} {local_res.css.mediateaser_photo} {local_res.css.mediateaser_photothumb}"></div>
            <div title="{event.photos[2].title}" data-src="{event.photos[2].file}" style="background-image: url(images/{event.photos[2].thumb});" class="{local_res.css.lightbox_photo} {local_res.css.mediateaser_photo} {local_res.css.mediateaser_photothumb}"></div>
          </div>
        </div>
      </div>
      <div class="{local_res.css.mediateaser} {local_res.css.mediateaservideos} {local_res.css.videowrapper}">
        <h3 class="{local_res.css.mediateaser_title}"><a href="event-media.html" class="{local_res.css.mediateaser_title_link}">Videos&nbsp;<span class="{local_res.css.mediateaser_title_value}">({event.videosCount})</span></a></h3>
        <div style="background-image: url(images/{event.videos[0].preview});" data-src="undefined" class="{local_res.css.mediateaser_video} {local_res.css.playablevideo}"><span class="{local_res.css.playablevideo_buttonplay}"></span></div>
      </div>
    </div>
    <div class="{res.mediaCss.small4} {res.mediaCss.columns}">{#if event.liveNow}<a href="{event.liveNow.regatta.series.race.link}" class="{local_res.css.box} {local_res.css.boxlink}">
        <div class="{local_res.css.box_header}">
          <h3 class="{local_res.css.box_header_title}">{event.liveNow.regatta.name}</h3>
          <div data-labeltype="live" class="{res.mainCss.label}">Live</div>
        </div><img src="images/{event.liveNow.preview}" alt=""></img>
        <div class="{local_res.css.box_content}">
          <h5>{event.liveNow.regatta.series.name}&nbsp;-&nbsp;{event.liveNow.regatta.series.race.name}
          </h5>{event.liveNow.regatta.series.race.details}
        </div></a>{/if event.liveNow}{#if event.upcoming}
      <div class="{local_res.css.box}">
        <div class="{local_res.css.box_header}">
          <h3 class="{local_res.css.box_header_title}">Upcoming Races Today</h3>
        </div>{#each race in event.upcoming.races}<a href="{race.link}" class="{local_res.css.box_content} {local_res.css.box_contentlink} {local_res.css.upcomingraceshort}">
          <div class="{local_res.css.upcomingraceshort_name}">{race.name}
            <div class="{local_res.css.upcomingraceshort_subtitle}"></div></div>
          <div data-countdown="{race.isCountdown}" class="{local_res.css.upcomingraceshort_details}">{#if race.isLive}
            <div data-labeltype="live" class="{res.mainCss.label}">Live</div>{else}{/if race.isLive}
          </div>
          <div class="{local_res.css.upcomingraceshort_countdown}">Starting in&nbsp;&nbsp;hrs and&nbsp;&nbsp;minutes&nbsp;
          </div></a>{/each event.upcoming.races}<a href="event-schedule.html" class="{local_res.css.box_content} {res.mainCss.morelink}">Event Schedule</a>
      </div>{/if event.upcoming}{#if event.weather}
      <div class="{local_res.css.box} {local_res.css.weathershort}">
        <div class="{local_res.css.weathershort_title}">
          <h3>Weather</h3>
        </div>
        <div class="{res.mediaCss.grid}">
          <div class="{res.mediaCss.small4} {res.mediaCss.columns} {local_res.css.weathershort_data} {local_res.css.weathershort_datawindspeed}"><span class="{local_res.css.weathershort_data_value}">{event.weather.windSpeed}</span><span class="{local_res.css.weathershort_data_unit}">&nbsp;KTS</span></div>
          <div class="{res.mediaCss.small4} {res.mediaCss.columns} {local_res.css.weathershort_data} {local_res.css.weathershort_datawinddirection}"><span class="{local_res.css.weathershort_data_value}">{event.weather.windDirection}</span>Â°</div>
          <div data-currentweather="{event.weather.current}" class="{res.mediaCss.small4} {res.mediaCss.columns} {local_res.css.weathershort_data} {local_res.css.weathershort_datatemperature}"><span class="{local_res.css.weathershort_data_value}">{event.weather.temperature}</span>Â°</div>
        </div>
      </div>{/if event.weather}{#if event.news}
      <div class="{local_res.css.box}">
        <div class="{local_res.css.box_header}">
          <h3 class="{local_res.css.box_header_title}">News Update</h3>
        </div>{#each message in event.news.messages}<a href="{message.link}" class="{local_res.css.box_content} {local_res.css.box_contentlink} {local_res.css.newsupdate}"><span class="{local_res.css.newsupdate_date}">{message.date}</span><span>{message.text}</span></a>{#each message in event.news.messages}<a href="#" class="{local_res.css.box_content} {res.mainCss.morelink}">Report news</a>
      </div>{/if event.news}
    </div>
  </div>
</div>
 -->
    </g:HTMLPanel>
</ui:UiBinder>
