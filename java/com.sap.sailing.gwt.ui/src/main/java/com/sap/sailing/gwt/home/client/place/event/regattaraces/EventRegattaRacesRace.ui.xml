<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
    <ui:with field="i18n" type="com.sap.sailing.gwt.home.client.i18n.TextMessages"/>
    <ui:with field="res" type="com.sap.sailing.gwt.home.client.HomeResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.shared.eventregattaraces.EventRegattaRacesRaceResources" />
    <g:HTMLPanel>

<div class="{local_res.css.eventregattarace}">
  <div style="background-color: undefined" class="{local_res.css.eventregattarace_fleetindicator}"></div>
  <div class="{local_res.css.eventregattarace_details} {local_res.css.eventregattarace_name}"><span class="{local_res.css.eventregattarace_nameshort}"></span>Race <br></br>{#if race.isFinished}<ui:text from='{i18n.eventRegattaRaceFinished}'/> {/if race.isFinished}<ui:text from='{i18n.eventRegattaRaceStarted}'/> <ui:text from='{i18n.eventRegattaRaceScheduled}'/> 
  </div>{#if race.isTracked}{/if race.isTracked}{#if race.isTracked}{/if race.isTracked}{#if race.isFinished}{/if race.isFinished}
</div>
    </g:HTMLPanel>
</ui:UiBinder>
