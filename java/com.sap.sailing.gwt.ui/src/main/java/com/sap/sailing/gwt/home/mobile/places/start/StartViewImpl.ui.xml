<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
    xmlns:ui="urn:ui:com.google.gwt.uibinder" 
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared"
    xmlns:mp="urn:import:com.sap.sailing.gwt.home.mobile.partials"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    >
    <ui:with 
        field="res" 
        type="com.sap.sailing.gwt.home.mobile.resources.SharedResources" />
    <ui:style>
        .headerWrapper {
            background-color: #f2f2f2;
        }
    </ui:style>    
    <g:HTMLPanel>
        <div class="{style.headerWrapper}">
            <mp:quickfinder.Quickfinder ui:field="quickFinderUi" />
        </div>
        <mp:stage.Stage ui:field='stage'/>
        <mp:socialfooter.SocialFooter/>    
    </g:HTMLPanel>
</ui:UiBinder>
