<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
    <ui:with field="i18n" type="com.sap.sailing.gwt.home.client.i18n.TextMessages"/>
    <ui:with field="res" type="com.sap.sailing.gwt.home.client.HomeResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.shared.header.HeaderResources" />
    <g:HTMLPanel>
<!-- Website header-->
	<g:HTMLPanel>
		<header class="{local_res.css.siteheader}">
			<div class="{res.mainCss.grid}">
				<div class="{res.mainCss.small4} {res.mainCss.columns}">
					<g:Image resource="{local_res.logo} altText="SAP Sailing Analytics" styleName="{local_res.css.logo}"/>
				</div>
				<div class="{res.mainCss.small8} {res.mainCss.columns}">
					<nav class="{local_res.css.sitenavigation}">
						<g:Anchor ui:field="startPageLink"
							styleName="{local_res.css.sitenavigation_link}" text="{i18n.home}" href="javascript:;"/>
						<g:Anchor ui:field="eventsPageLink"
							styleName="{local_res.css.sitenavigation_link}" text="{i18n.events}" href="javascript:;"/>
						<g:Anchor ui:field="aboutUsPageLink"
							styleName="{local_res.css.sitenavigation_link}" text="{i18n.aboutUs}" href="javascript:;"/>
						<g:Anchor ui:field="contactPageLink"
							styleName="{local_res.css.sitenavigation_link} {local_res.css.sitenavigation_linksearch}" text="{i18n.search}" href="javascript:;">
							<img src="images/search-icon.png" alt="Search">
						</g:Anchor>
					</nav>
				</div>
			</div>
		</header>
	</g:HTMLPanel>
</ui:UiBinder>