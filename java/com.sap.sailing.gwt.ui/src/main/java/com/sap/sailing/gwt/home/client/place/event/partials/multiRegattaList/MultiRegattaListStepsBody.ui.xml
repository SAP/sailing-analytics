	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
	<ui:with field="local_res" type="com.sap.sailing.gwt.home.client.place.event.partials.multiRegattaList.MultiRegattaListResources" />
	<div style="width: 33.333%; min-width:225px;" class="{local_res.css.regattalistitem_steps_step}">
		<div class="{local_res.css.regattalistitem_steps_step_infowrapper}">
			<div class="{local_res.css.regattalistitem_steps_step_name}" ui:field="nameUi"></div>
			<div class="{local_res.css.regattalistitem_steps_step_check}" ui:field="checkUi"></div>
			<div class="{local_res.css.regattalistitem_steps_step_progress}" ui:field="progressUi"></div>
