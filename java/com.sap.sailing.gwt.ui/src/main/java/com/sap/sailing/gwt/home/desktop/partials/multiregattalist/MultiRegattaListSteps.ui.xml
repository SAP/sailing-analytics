<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" 
		xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared"	
		xmlns:dp="urn:import:com.sap.sailing.gwt.home.desktop.partials">
	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
	<ui:with field="local_res" type="com.sap.sailing.gwt.home.desktop.partials.multiregattalist.MultiRegattaListResources" />
	<g:HTMLPanel>
		<div class="{local_res.css.regattalistitem_steps}" ui:field="progressContainerUi">
			<div ui:field="noRacesMessageUi" class="{local_res.css.regattalistitem_noraces}" style="display: none;"><ui:text from="{i18n.noRacesYet}" /></div>
			<div ui:field="stepsContainerUi" />
			<div class="{local_res.css.regattalistitem_steps_button}" ui:field="leaderboardButtonContainerUi" style="display:none;">
				<a class="{res.mainCss.button} {res.mainCss.buttonstrong} {res.mainCss.buttonarrowrightwhite}" 
						ui:field="leaderboardButtonUi">
					<ui:text from='{i18n.leaderboard}' />
				</a>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>