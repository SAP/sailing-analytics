<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
  <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.place.event.partials.countdownTimer.CountdownTimerResources" />
    <div class="{local_res.css.countdowntimer}">
      <div class="{local_res.css.countdowntimer_content}">
        <div ui:field="majorContainer" class="{local_res.css.countdowntimer_content_pane} {local_res.css.countdowntimer_content_panefirst}">
          <div ui:field="majorValue" class="{local_res.css.countdowntimer_content_pane_count}"></div>
          <div ui:field="majorUnit" class="{local_res.css.countdowntimer_content_pane_unit}"></div>
        </div>
        <div ui:field="minorContainer" class="{local_res.css.countdowntimer_content_pane} {local_res.css.countdowntimer_content_panesecond}">
          <div ui:field="minorValue" class="{local_res.css.countdowntimer_content_pane_count}"></div>
          <div ui:field="minorUnit" class="{local_res.css.countdowntimer_content_pane_unit}"></div>
        </div>
      </div>
    </div>
</ui:UiBinder>