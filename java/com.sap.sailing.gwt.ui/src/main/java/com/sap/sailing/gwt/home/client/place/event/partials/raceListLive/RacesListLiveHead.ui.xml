<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
		xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared"
		xmlns:r="urn:import:com.sap.sailing.gwt.home.client.place.event.partials.raceListLive">
		<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
		<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
		<ui:with field="i18n_ubi" type="com.sap.sailing.gwt.common.client.i18n.TextMessages" />
		<ui:with field="local_res" type="com.sap.sailing.gwt.home.client.place.event.partials.raceListLive.RacesListLiveResources" />
	<g:HTMLPanel tag="thead">
	 <tr class="{local_res.css.raceslist_head}">
      <th class="{local_res.css.raceslist_head_item}">
      </th>
      <th class="{local_res.css.raceslist_head_item}">
        <ui:text from='{i18n.regatta}' />
      </th>
      <th class="{local_res.css.raceslist_head_item}">
        <ui:text from='{i18n.race}' />
      </th>
      <th class="{local_res.css.raceslist_head_item} {res.mediaCss.showonlarge}">
        <ui:text from='{i18n.fleet}' />
      </th>
      <th class="{local_res.css.raceslist_head_item}">
        <ui:text from='{i18n.start}' />
      </th>
      <th class="{local_res.css.raceslist_head_item} {res.mediaCss.showonlarge}">
		TODOFLAG <!-- <ui:text from='{i18n.flag}' /> -->
      </th>
      <th class="{local_res.css.raceslist_head_item}">
        <ui:text from='{i18n.wind}' />
      </th>
      <th class="{local_res.css.raceslist_head_item} {res.mediaCss.hideonsmall}">
        <ui:text from='{i18n.from}' />
      </th>
      <th class="{local_res.css.raceslist_head_item} {res.mediaCss.showonlarge}">
		TODOAREA <!-- <ui:text from='{i18n.area}' /> -->
      </th>
      <th class="{local_res.css.raceslist_head_item} {res.mediaCss.showonlarge}">
        <ui:text from='{i18n.course}' />
      </th>
      <th class="{local_res.css.raceslist_head_item}">
        <ui:text from='{i18n.status}' />
      </th>
      <th class="{local_res.css.raceslist_head_item} {local_res.css.raceslist_head_itembutton}">
		TODOWATCH <!-- <ui:text from='{i18n_ubi.i18nRegattaHeadersButton}' /> -->
      </th>
     </tr>
    </g:HTMLPanel>
</ui:UiBinder>