<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
		xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared"	xmlns:mp="urn:import:com.sap.sailing.gwt.home.mobile.partials">
	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
	<ui:with field="local_res" type="com.sap.sailing.gwt.home.mobile.partials.regattaStatus.RegattaStatusResources" />
	
	<g:FlowPanel>
		<mp:section.MobileSection ui:field="itemContainerUi">
			<mp:header>
				<mp:sectionHeader.SectionHeaderContent sectionTitle="{i18n.regattas}" />
			</mp:header>
			<mp:content>
				<mp:section.MobileSection addStyleNames="{local_res.css.regattastatus_content}" ui:field="regattaContainerUi"/>
			</mp:content>
			<mp:content>
				<mp:section.MobileSection addStyleNames="{local_res.css.regattastatus_content}" ui:field="collapsableContainerUi"/>
			</mp:content>
		</mp:section.MobileSection>
		<mp:toggleButton.ToggleButton ui:field="toggleButtonUi" label="{i18n.regattas}"/>
	</g:FlowPanel>
</ui:UiBinder>