<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
    <ui:with field="i18n" type="com.sap.sailing.gwt.home.client.i18n.TextMessages"/>
    <ui:with field="res" type="com.sap.sailing.gwt.home.client.HomeResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.place.event.regatta.RegattaResources" />
    <g:HTMLPanel>

<div class="{local_res.css.regatta_intro_phases_phase}">
  <div class="{local_res.css.regatta_intro_phases_phase_name}"><span ui:field="phaseName" /></div>
  <g:HTMLPanel ui:field="phaseRacesPanel" styleName="{local_res.css.regatta_intro_phases_phase_races}" />
</div>
    </g:HTMLPanel>
</ui:UiBinder>
