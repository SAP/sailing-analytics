<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
    <ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
	<ui:with field="local_res"
		type="com.sap.sailing.gwt.home.mobile.partials.upcoming.EventsOverviewUpcomingResources" />
	<ui:with field="shr" type="com.sap.sailing.gwt.home.shared.resources.SharedHomeResources" />
	<ui:style>
	.holder {
		
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.holder}">
		<div class="{local_res.css.eventsoverviewupcoming_content_event}">
			<g:Anchor ui:field="eventOverviewLink"
				addStyleNames="{res.mediaCss.grid} {local_res.css.eventsoverviewupcoming_content_event_link}">
				<div
					class="{local_res.css.eventsoverviewupcoming_content_event_link_body} {res.mediaCss.small10} {res.mediaCss.columns}">
					<div ui:field='eventName'
						class="{local_res.css.eventsoverviewupcoming_content_event_link_body_name}" />
					<div
						class="{local_res.css.eventsoverviewupcoming_content_event_link_body_location}">
						<span ui:field='venueName' />, <span ui:field='eventStartDate' />
					</div>
				</div>
				<div
					class="{local_res.css.eventsoverviewupcoming_content_event_link_arrow} {res.mediaCss.small2} {res.mediaCss.columns}">
					<img src="{shr.arrowDownGrey.getSafeUri}"
						class="{local_res.css.eventsoverviewupcoming_content_event_link_arrow_image}"></img>
				</div>
			</g:Anchor>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
