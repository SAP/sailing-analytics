<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.desktop.partials.regattaheader.RegattaHeaderResources" />
    <g:HTMLPanel>
	    <table>
	        <tr>
	            <td><div ui:field="iconGPS" class="{local_res.css.iconGPS}"/></td>
	            <td><div ui:field="txtGPS"/></td>
	            <td><div ui:field="iconWind" class="{local_res.css.iconWind}"/></td>
	            <td><div ui:field="txtWind"/></td>
	        </tr>
	        <tr>
	            <td><div ui:field="iconVideo" class="{local_res.css.iconVideo}"/></td>
	            <td><div ui:field="txtVideo"/></td>
	            <td><div ui:field="iconAudio" class="{local_res.css.iconAudio}"/></td>
	            <td><div ui:field="txtAudio"/></td>
	       </tr>
	    </table>
    </g:HTMLPanel>
</ui:UiBinder> 