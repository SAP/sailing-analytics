<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="res" type="com.sap.sailing.gwt.home.shared.partials.dialog.DialogResources" />
    <g:HTMLPanel>
        <div class="{res.css.message}" ui:field="messageField" />
        <div class="{res.css.buttonPanel}">
            <g:Button styleName="{res.css.yesButton} {res.css.button}" ui:field="yesButton">Yes</g:Button>
            <g:Button styleName="{res.css.noButton} {res.css.button}" ui:field="noButton">No</g:Button>
        </div>
    </g:HTMLPanel>
</ui:UiBinder> 