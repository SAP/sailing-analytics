}
.solutions_contenttraining .parallax {
  font-size: 1rem;
}
.solutions_contenttraining .parallax .background {
  margin: auto;
  height: 100%;
  max-width: 80em;
  background-repeat: no-repeat;
  background-image: url("images/home/	-training.png");
  background-size: 100% auto;
  background-position: center 13.333333333333334em;
}
.solutions_contenttraining .parallax.noparallax .background {
  background-position: center 6.666666666666667em;
}
.solutions_contentsimulator {
  font-size: 1rem;
}
.solutions_contentsimulator .gridalternator {
  background-repeat: no-repeat;
  background-image: url("images/home/solutions-simulator.png");
  background-position: left center;
  background-size: 100%;
  padding: 3.333333333333333em 0 3.333333333333333em;
}
@media (min-width: 45em) {
  .solutions_contentsimulator .gridalternator {
    padding: 5em 0 6.666666666666667em;
  }
}
@media (min-width: 61em) {
  .solutions_contentsimulator .gridalternator {
