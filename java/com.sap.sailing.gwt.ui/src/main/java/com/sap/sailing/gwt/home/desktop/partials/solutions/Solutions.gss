/**
 * The solutions page
 */
.solutions {
  font-size: 1rem;
}
.solutions_nav {
  position: absolute;
 
  right: 0;
  left: 0;
  border-bottom: 1px solid #f2f2f2;
  background: #fff;
  z-index: 10;
  text-align: center;
}
.solutions_nav_link {
  display: inline-block;
  padding: 1em 0 0.733333333333333em 0;
  font-weight: 600;
  color: #666;
  -webkit-transition: 0.2s border ease;
     -moz-transition: 0.2s border ease;
          transition: 0.2s border ease;
  margin-right: 1em;
}
.solutions_nav_link:hover,
.solutions_nav_linkactive {
  text-decoration: none;
	color: #666;
	border-bottom: 4px solid #efaa00;
	margin-bottom: -1px
}
.solutions_nav_link:last-child {
  margin-right: 0;
}
@media (min-width: 45em) {
  .solutions_nav_link {
    margin-right: 1em;
  }
}
@media (min-width: 61em) {
  .solutions_nav_link {
    margin-right: 2em;
  }
}
.solutions_content {
  position: relative;
  z-index: 2;
}
.solutions_content:nth-child(even) {
  background-color: #f8f8f8;
}
.solutions_content:nth-child(odd) {
  background-color: #fff;
}
.solutions_content .parallax {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: -1;
}
.solutions_content .gridalternator {
  height: 100%;
  padding: 3.333333333333333em 0;
}
@media (min-width: 45em) {
  .solutions_content .gridalternator {
    padding: 5em 0;
  }
}
@media (min-width: 61em) {
  .solutions_content .gridalternator {
    padding: 6.666666666666667em 0;
  }
}
.solutions_contentsapinsailing {
  width: 100%;
}
.solutions_contentsapinsailing_body {
  margin: auto;
  padding-right: 50%;
  max-width: 88em;
  background-repeat: no-repeat;
  background-position: right center;
  background-size: contain;
  background-image: url("images/home/solutions-sap-in-sailing.jpg");
}
.solutions_contentsapinsailing_body .gridalternator {
  padding: 4em 0 4.333333333333333em;
}
@media (min-width: 45em) {
  .solutions_contentsapinsailing_body .gridalternator {
    padding: 5em 0 5.666666666666667em;
  }
}
@media (min-width: 61em) {
  .solutions_contentsapinsailing_body .gridalternator {
    padding: 6.666666666666667em 0 12em;
  }
}
.solutions_contentsap {
  max-width: 88em;
  margin: auto;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: 100%;
  background-image: url("images/home/solutions-sap.png");
}
.solutions_contentsap .gridalternator {
  padding: 4em 0 4.333333333333333em;
}
@media (min-width: 45em) {
  .solutions_contentsap .gridalternator {
    padding: 5em 0 5.666666666666667em;
  }
}
@media (min-width: 61em) {
  .solutions_contentsap .gridalternator {
    padding: 6.666666666666667em 0 12em;
  }
}
.solutions_contentsapsailingracemanager {
  width: 100%;
}
.solutions_contentsapsailingracemanager_body {
  margin: auto;
  padding: 4em 50% 4.333333333333333em 0;
  max-width: 88em;
  background-repeat: no-repeat;
  background-image: url("images/home/solutions-sap-sailing-race-manager.png");
  background-size: 40%;
  background-position: center right;
}
@media (min-width: 45em) {
  .solutions_contentsapsailingracemanager_body .gridalternator {
    padding: 5em 0 5.666666666666667em;
  }
}
@media (min-width: 61em) {
  .solutions_contentsapsailingracemanager_body .gridalternator {
    padding: 6.666666666666667em 0 12em;
  }
}
.solutions_contentpost {
  font-size: 1rem;
}
.solutions_contentpost .gridalternator {
  background-repeat: no-repeat;
  background-image: url("images/home/solutions-post.png");
  background-position: left center;
  background-size: 100%;
  padding: 1.666666666666667em 0;
}
@media (min-width: 45em) {
  .solutions_contentpost .gridalternator {
    padding: 3.333333333333333em 0;
  }
}
@media (min-width: 61em) {
  .solutions_contentpost .gridalternator {
    padding: 5em 0;
  }
}
.solutions_contenttraining {
  font-size: 1rem;
}
.solutions_contenttraining .gridalternator {
  padding-top: 5em;
}
@media (min-width: 45em) {
  .solutions_contenttraining .gridalternator {
    padding-top: 6.666666666666667em;
  }
}
@media (min-width: 61em) {
  .solutions_contenttraining .gridalternator {
