<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
	<ui:with field="local_res" type="com.sap.sailing.gwt.home.shared.partials.lowerthird.LowerThirdResources" />
	<div class="{res.mediaCss.grid}">
		<div class="{local_res.css.video_content_item_info} {local_res.css.info}">
			<div ui:field="titleWrapperUi" class="{local_res.css.video_content_item_info_text} {res.mediaCss.columns} {res.mediaCss.small12}">
				<h3 class="{local_res.css.video_content_item_info_text_title} {local_res.css.video_content_item_info_text_titlesmall}">
					<span ui:field="titleUi" />
					<div ui:field="labelUi" class="{res.mainCss.label} {local_res.css.label}" style="display: none;" />
				</h3>
				<p ui:field="subtitleUi" style="display: none;"></p>
			</div>
			<div ui:field="buttonWrapperUi" class="{res.mediaCss.small4} {res.mediaCss.columns}" style="display: none;">
				<a ui:field="buttonUi" class="{local_res.css.video_content_item_info_button} {res.mainCss.button} {res.mainCss.buttonsmall} {res.mainCss.buttonstrong} {res.mainCss.buttonprimary} {res.mainCss.buttonarrowrightwhite}" />
			</div>
		</div>
	</div>
</ui:UiBinder>