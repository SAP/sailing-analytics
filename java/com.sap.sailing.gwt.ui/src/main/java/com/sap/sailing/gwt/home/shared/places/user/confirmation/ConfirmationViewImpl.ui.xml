<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:sp="urn:import:com.sap.sailing.gwt.home.shared.partials">
    <ui:with field="res" type="com.sap.sse.security.ui.authentication.UserManagementSharedResources" />
    <g:HTMLPanel addStyleNames="{res.mainCss.mainsection}">
        <div class="{res.mainCss.mainsection_header}">
            <div class="{res.mainCss.mainsection_header_title}" ui:field="messageTitleUi" />
            <p ui:field="messageUi" />
        </div>
    </g:HTMLPanel>
</ui:UiBinder>    
