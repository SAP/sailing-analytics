.accordion {
  font-size: 1rem;
}
.accordion_trigger {
  font-size: inherit;
}
.accordion_content {
  font-size: inherit;
}
.regattacompetition {
  font-size: 1rem;
  padding: 0;
}
.regattacompetition .grid {
  margin: 0;
}
.regattacompetition_phase {
  font-size: 0;
}
.regattacompetition_phase.accordioncollapsed .sectionheader_item_togglearrow {
  -webkit-transform: rotate(0deg);
     -moz-transform: rotate(0deg);
      -ms-transform: rotate(0deg);
          transform: rotate(0deg);
}
.regattacompetition_phase .sectionheader_item_togglearrow {
  -webkit-transform: rotate(180deg);
     -moz-transform: rotate(180deg);
      -ms-transform: rotate(180deg);
          transform: rotate(180deg);
  -webkit-transition: -webkit-transform 0.4s;
     -moz-transition: -moz-transform 0.4s;
          transition: transform 0.4s;
}
.regattacompetition_phase_fleet {
  font-size: 1rem;
  display: inline-block;
  position: relative;
  padding: 1.333333333333333em 0.333333333333333em 0.333333333333333em 0.333333333333333em;
  vertical-align: top;
}
/* .regattacompetition_phase_fleet:first-child { */
/*   padding-left: 0.666666666666667em; */
/* } */
/* .regattacompetition_phase_fleet:last-child { */
/*   padding-right: 0.666666666666667em; */
/* } */
.regattacompetition_phase_fleetfullwidth .regattacompetition_phase_fleet_race {
  height: 3.466666666666666666em;
  text-align: center;
}
.regattacompetition_phase_fleetfullwidth .regattacompetition_phase_fleet_race_title {
  text-overflow: ellipsis;
  position: absolute;
  top: 50%;
  left: 1.133333333333333em;
  right: 2.133333333333333em;
  text-align: left;
  -webkit-transform: translateY(-50%);
     -moz-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
          transform: translateY(-50%);
}
.regattacompetition_phase_fleetfullwidth .regattacompetition_phase_fleet_race_title:after {
  display: none;
}
/* .regattacompetition_phase_fleetfullwidth .regattacompetition_phase_fleet_race_subtitle { */
/*   font-size: 0.866666666666667em; */
/* } */
.regattacompetition_phase_fleetfullwidth .regattacompetition_phase_fleet_race_arrow {
  display: block;
  width: 1.6666666667em;
  height: 1.4444444444em;
}
.regattacompetition_phase_fleet_title {
  font-size: 1.066666666666667em;
  font-weight: 700;
  min-height: 2.8125em;
  margin-bottom: 0.375em;
  text-align: center;
}
.regattacompetition_phase_fleetcompact .regattacompetition_phase_fleet_title {
  font-size: 0.966666666666667em;
}
.regattacompetition_phase_fleetcompact .regattacompetition_phase_fleet_race_title:after {
  width: 1.066666666666667em;
}
.regattacompetition_phase_fleet_race {
  position: relative;
  background: #008fcc;
  margin-bottom: 0.666666666666667em;
/*   text-align: center; */
  padding: .8em .4em;
  border-radius: 0.333333333333333em;
  color: #fff;
  /* color: #f6f9fc; alternative white*/
  height: 5em;  
}
.regattacompetition_phase_fleet_racetracked {
  background: #008fcc;
}
.regattacompetition_phase_fleet_raceuntracked {
  color: #008fcc;
  background-color: #fff;
  border: 0.133333333333333em solid #008fcc;
  padding: .666666666666667em 0.266666666666667em;
  cursor: default;
}
.regattacompetition_phase_fleet_raceuntracked .regattacompetition_phase_fleet_race_title:after {
  display: none;
}
.regattacompetition_phase_fleetfullwidth .regattacompetition_phase_fleet_raceuntracked .regattacompetition_phase_fleet_race_title {
  left: 1em;
}
.regattacompetition_phase_fleet_racelive {
  background-color: #e94a1b;
}
.regattacompetition_phase_fleet_raceplanned {
  background-color: #999;
  font-style: italic;
}
.regattacompetition_phase_fleet_racelive.regattacompetition_phase_fleet_raceuntracked {
  color: #e94a1b;
  background-color: #fff;
  border-color: #e94a1b;
}
.regattacompetition_phase_fleet_raceplanned.regattacompetition_phase_fleet_raceuntracked {
  background-color: #fff;
  color: #999;
  border-color: #999;
}
.regattacompetition_phase_fleet_race_title {
  font-size: 1.222222222222222em;
  font-weight: 700;
  position: relative;
}
.regattacompetition_phase_fleet_race_title:after {
  content: '';
  display: inline-block;
  width: 1.666666666666667em;
  height: 1.1em;
  background: resourceUrl("launchLoupe") no-repeat center center;
  position: absolute;
  right: 0;
  top: 0.133333333333333em;
}
.regattacompetition_phase_fleet_race_subtitle {
  font-size: 0.7em;
}
.regattacompetition_phase_fleet_race_arrow {
  display: none;
  background: resourceUrl("launchLoupe") no-repeat center center;
  position: absolute;
  top: 50%;
  right: 1em;
  -webkit-transform: translateY(-50%);
     -moz-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
          transform: translateY(-50%);
}
.regattacompetition_phase_fleet_raceplanned .regattacompetition_phase_fleet_race_title:after {
  background: resourceUrl("launchPlay") no-repeat center center;
}
.regattacompetition_phase_fleet_raceplanned .regattacompetition_phase_fleet_race_arrow {
  background: resourceUrl("launchPlay") no-repeat center center;
}
.regattacompetition_phase_fleet_raceuntracked .regattacompetition_phase_fleet_race_arrow {
  display: none;
}
.regattacompetition_phase_fleet_race_state {
  text-transform: uppercase;
}
.regattacompetition_phase_fleet_race_date {
}
.regattacompetition_phase_fleet_race_title,
.regattacompetition_phase_fleet_race_state,
.regattacompetition_phase_fleet_race_date {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.regattacompetition_phase_fleet_race_title_big {
  width: 27%;
}

@media (min-width: 375px) {
  .regattacompetition_phase_fleet_race_title_big {
    width: 33%;
  }
}

@media (min-width: 700px) {
  .regattacompetition_phase_fleet_race_title_big {
    width: 40%;
  }
}
