<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
  		xmlns:sp="urn:import:com.sap.sailing.gwt.home.shared.partials"> 
  	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
    <ui:with field="res_home" type="com.sap.sailing.gwt.home.shared.SharedHomeResources" />
    <ui:with field="media_res" type="com.sap.sailing.gwt.home.mobile.places.event.media.MediaViewResources" />
	<ui:with field="local_res" type="com.sap.sailing.gwt.home.mobile.partials.videogallery.VideoGalleryResources" />
	<g:HTMLPanel addStyleNames="{res.mediaCss.small12} {res.mediaCss.columns} {local_res.css.videogallery_video}">
		<div class="{local_res.css.video}">
			<sp:videoplayer.VideoPlayer ui:field="videoPlayerUi"/>
		</div>
		<div class="{local_res.css.videogallery_video_info}">
			<div class="{local_res.css.videogallery_video_info_title}" ui:field="videoTitleUi"></div>
			<div class="{local_res.css.videogallery_video_info_subtitle}" ui:field="videoCreateDateUi"></div>
		</div>
        <div ui:field="overlayUi" class="{res_home.sharedHomeCss.overlay}">
            <g:Button addStyleNames="{media_res.css.button} {media_res.css.editButton}" ui:field="editButtonUi"></g:Button>
            <g:Button addStyleNames="{media_res.css.button} {media_res.css.deleteButton}" ui:field="deleteButtonUi"></g:Button>
        </div>
	</g:HTMLPanel>
</ui:UiBinder>