<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
    <ui:with field="i18n" type="com.sap.sailing.gwt.common.client.i18n.TextMessages"/>
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.place.event2.partials.eventregatta.EventRegattaListResources" />
    <g:HTMLPanel>
<!-- A list of regattas-->
<div class="{local_res.css.eventregattalist}">
    <div ui:field="regattaListNavigationDiv" class="{local_res.css.eventregattalist_navigation} {res.mainCss.navbar}">
    <div class="{res.mediaCss.grid}">
      <div ui:field="regattaGroupsNavigationPanel" class="{res.mediaCss.small12} {res.mediaCss.columns}">
          <a ui:field="filterNoLeaderboardGroupsAnchor" href="javascript:;" class="{res.mainCss.navbar_button} {res.mainCss.navbar_buttonactive}"><span ui:field="allBoatClassesSelected">&#10003;&nbsp;</span><ui:text from='{i18n.eventRegattaListAllBoatClasses}'/></a>
      </div>
    </div>
  </div>
  <g:HTMLPanel ui:field="regattaListItemsPanel" />
</div>        
    </g:HTMLPanel>
</ui:UiBinder>
