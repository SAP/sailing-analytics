<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" 
		xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared"	
		xmlns:dp="urn:import:com.sap.sailing.gwt.home.desktop.partials">
	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
	<ui:with field="local_res" type="com.sap.sailing.gwt.home.desktop.partials.multiregattalist.MultiRegattaListResources" />
	<div class="{local_res.css.regattalistitem_steps_step}">
		<div class="{local_res.css.regattalistitem_steps_step_infowrapper}">
			<div class="{local_res.css.regattalistitem_steps_step_textwrapper}" ui:field="textContainerUi">
				<div class="{local_res.css.regattalistitem_steps_step_name_dummy}" ui:field="longNameDummyUi"></div>
				<div class="{local_res.css.regattalistitem_steps_step_name_dummy}" ui:field="mediumNameDummyUi"></div>
				<div class="{local_res.css.regattalistitem_steps_step_name}" ui:field="nameUi"></div>
				<div class="{local_res.css.regattalistitem_steps_step_check}" ui:field="checkUi"></div>
				<div class="{local_res.css.regattalistitem_steps_step_progress}" ui:field="progressUi"></div>
			</div>
			<div class="{local_res.css.regattalistitem_steps_step_fleets}" ui:field="fleetsContainerUi"></div>
		</div>
	</div>
</ui:UiBinder>