<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:sp="urn:import:com.sap.sailing.gwt.home.shared.partials">
    <ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
    <ui:with field="local_res"
        type="com.sap.sailing.gwt.home.shared.partials.multiselection.SuggestedMultiSelectionResources" />
    <ui:with field="e_res" type="com.sap.sailing.gwt.home.shared.partials.editable.EditableResources" />
    <ui:with field="dar"
        type="com.sap.sailing.gwt.home.desktop.partials.desktopaccordion.DesktopAccordionResources" />

    <g:HTMLPanel addStyleNames="{local_res.css.suggestions}" ui:field="headerPanelUi">
        <div class="{local_res.css.suggestionsHeader}" ui:field="headerElementUi">
            <span class="{local_res.css.suggestionsHeaderTitle}" ui:field="headerTitleUi" />
            <sp:editable.InlineEditButton
                addStyleNames="{e_res.css.editableSuggestionEditButton}" ui:field="editButtonUi" />
        </div>
        <g:Label ui:field="competitorsEmptyUi" addStyleNames="{dar.css.accordionEmptyMessage}"
            text="{i18n.pleaseSelectCompetitorFirst}" visible="false" />
        <div class="{local_res.css.suggestionsContent}" ui:field="parentPanelUi">
            <g:FlowPanel addStyleNames="{local_res.css.suggestionsContentTable}" ui:field="itemContainerUi" />
            <sp:multiselection.SuggestedMultiSelectionView
                ui:field="multiSelect" />
        </div>
    </g:HTMLPanel>
</ui:UiBinder> 