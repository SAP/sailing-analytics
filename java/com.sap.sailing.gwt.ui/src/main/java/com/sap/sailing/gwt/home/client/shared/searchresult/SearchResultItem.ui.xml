<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
    <ui:with field="i18n" type="com.sap.sailing.gwt.home.client.i18n.TextMessages"/>
    <ui:with field="res" type="com.sap.sailing.gwt.home.client.HomeResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.shared.searchresult.SearchResultResources" />
    <g:HTMLPanel>
        <div class="{local_res.css.searchresult_content_item}">
            <g:Anchor ui:field="regattaLink" styleName="{local_res.css.searchresult_content_item_headline}" href="javascript:;" />
          <div class="{local_res.css.searchresult_content_item_description}"><span ui:field="resultRegattaDetails" />
				<!-- <a href="http://" class="{local_res.css.searchresult_content_item_description_morelink}">More</a>-->
          </div>
          <div class="{local_res.css.searchresult_content_item_footer}">
              <g:Anchor ui:field="eventOverviewLink" styleName="{local_res.css.searchresult_content_item_footer_link}" href="javascript:;" />
              <span class="{local_res.css.searchresult_content_item_footer_separator}">|</span><span ui:field="resultEventVenue" />
              <span class="{local_res.css.searchresult_content_item_footer_separator}">|</span><span ui:field="resultEventDate" />
          </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
