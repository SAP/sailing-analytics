<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" >
	<ui:with field="local_res" type="com.sap.sailing.gwt.home.client.place.event.partials.countdown.CountdownResources" />
	<g:HTMLPanel ui:field="countdown" addStyleNames="{local_res.css.countdown_content_panewrapper}">
		<div ui:field="majorContainer" class="{local_res.css.countdown_content_pane} {local_res.css.countdown_content_panefirst}">
			<div ui:field="majorValue" class="{local_res.css.countdown_content_pane_count}"></div>
			<div ui:field="majorUnit" class="{local_res.css.countdown_content_pane_unit}"></div>
		</div>
		<div ui:field="minorContainer" class="{local_res.css.countdown_content_pane} {local_res.css.countdown_content_panesecond}">
			<div ui:field="minorValue" class="{local_res.css.countdown_content_pane_count}"></div>
			<div ui:field="minorUnit" class="{local_res.css.countdown_content_pane_unit}"></div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>