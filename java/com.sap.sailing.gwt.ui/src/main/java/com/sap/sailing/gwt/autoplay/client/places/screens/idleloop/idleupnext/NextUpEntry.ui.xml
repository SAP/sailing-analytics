<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
    <ui:style>
    .newsTime {
    	display: inline-block;
    	font-weight: bold;
    	color: #008fcc;
    	padding: 10px;
    }
    
    .newsText {
    	display: inline-block;
        padding: 10px;
    }
    </ui:style>
    <div>
        <div class="{style.newsTime}" ui:field="newsTimeUi" />
        <div class="{style.newsText}" ui:field="newsTextUi" />
    </div>
</ui:UiBinder>