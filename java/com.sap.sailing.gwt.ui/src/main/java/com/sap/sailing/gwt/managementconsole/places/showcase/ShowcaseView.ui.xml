            </div>
            <div class="{res.mediaCss.column} {res.mediaCss.small12}">
                <div class="{app_res.style.secondaryCtaButton}">
                    <g:Button ui:field="forgotPasswordButton"
                        text="{i18n.forgotPassword}" />
                </div>
            </div>
        </div>
        <div class="{res.mediaCss.grid}">
            <div class="{res.mediaCss.column} {res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.large4}">
                <div class="{local_res.style.eventCard}">
                    <div class="{res.mediaCss.grid} {local_res.style.eventCardContainer}">
                        <div class="{res.mediaCss.column} {res.mediaCss.hideonsmall} {res.mediaCss.medium4}">
