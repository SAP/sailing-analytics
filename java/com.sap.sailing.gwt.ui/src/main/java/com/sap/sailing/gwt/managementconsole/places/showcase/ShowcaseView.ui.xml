            </div>
        </div>
        <div class="{res.mediaCss.grid}">
            <div class="{res.mediaCss.column} {res.mediaCss.small12} {res.mediaCss.medium6}">
                <label for="username">
                    USERNAME
                </label>
                <input id="username" placeholder="Username" />
            </div>
            <div class="{res.mediaCss.column} {res.mediaCss.small12} {res.mediaCss.medium6}">
                <label for="password">
                    PASSWORD
                </label>
                <input id="password" placeholder="Password" type="password" />
            </div>
        </div>
        <div class="{res.mediaCss.grid}">
            <div class="{res.mediaCss.column} {res.mediaCss.small12} {res.mediaCss.mediumoffset4} {res.mediaCss.medium4} {res.mediaCss.largeoffset6} {res.mediaCss.large3}">
                <g:Button ui:field="settingsButton"
                    text="{i18n.releaseNotes}"
                    styleName="{app_res.style.primaryButton}" />
            </div>
            <div class="{res.mediaCss.column} {res.mediaCss.small12} {res.mediaCss.medium4} {res.mediaCss.large3}">
                <g:Button ui:field="createRegattaLeaderboardButton"
                    text="{i18n.ok}"
