<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared" xmlns:v="urn:import:com.sap.sailing.gwt.ui.client.media">
  <ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
    <ui:with field="i18n_ubi" type="com.sap.sailing.gwt.common.client.i18n.TextMessages"/>
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
  <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.place.event.partials.video.VideoResources" />
  <g:HTMLPanel>
    <div class="{local_res.css.video}">
      <div class="{local_res.css.video_content}">
        <div class="{local_res.css.video_content_item} {local_res.css.videoitem}">
          <div class="{local_res.css.videoplaceholder} {res.mediaCss.small12}">
          	<v:VideoJSPlayer addStyleNames="{local_res.css.videoplaceholder_image}" ui:field="videoPlayer"/>
          </div>
        </div>
      </div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>