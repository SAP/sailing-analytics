<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
    <ui:with field="i18n" type="com.sap.sailing.gwt.home.client.i18n.TextMessages"/>
    <ui:with field="res" type="com.sap.sailing.gwt.home.client.HomeResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.place.event.regatta.RegattaResources" />
    <ui:style>
    .panel {
      display: inline-block;
    }
  	</ui:style>
    <g:HTMLPanel styleName="{style.panel}">
<div class="{local_res.css.regatta_intro_results_competitor}">
    <span ui:field="competitorRank" />.&nbsp;<span ui:field="competitorNationality" />&nbsp;-&nbsp;<span class="{local_res.css.regatta_intro_results_competitor_name}"><span ui:field="competitorName" /></span>&nbsp;-&nbsp;<span ui:field="competitorTotalPoints" /><ui:text from='{i18n.regattaPts}'/>
</div>
    </g:HTMLPanel>
</ui:UiBinder>
