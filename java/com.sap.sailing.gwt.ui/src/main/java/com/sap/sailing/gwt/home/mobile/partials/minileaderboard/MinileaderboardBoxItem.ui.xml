<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
	<ui:with field="res" type="com.sap.sailing.gwt.home.mobile.resources.SharedResources" />

	<ui:style>
	.outerBox {
		border-top: 1px solid grey;
		border-bottom: 1px solid grey;
		margin: 0 !important;
		position: relative;
		padding: 1.333333333333333em 0;
	}
	</ui:style>

	<div class="{res.mediaCss.grid} {style.outerBox}">
		<div class="{res.mediaCss.small1} {res.mediaCss.columns}" ui:field="competitorRankUi" />
		<div class="{res.mediaCss.small2} {res.mediaCss.columns}">
			<img ui:field="competitorFlagUi" />
		</div>
		<div class="{res.mediaCss.small7} {res.mediaCss.columns}" ui:field="competitorNameUi" />
		<div class="{res.mediaCss.small2} {res.mediaCss.columns}" ui:field="competitorPointsUi" />
		<div class="{res.mediaCss.smalloffset3} {res.mediaCss.small9} {res.mediaCss.columns}" ui:field="competitorCountryNameUi" />
	</div>

</ui:UiBinder>