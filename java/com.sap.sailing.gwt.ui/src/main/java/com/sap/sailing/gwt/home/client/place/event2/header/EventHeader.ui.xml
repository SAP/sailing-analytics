<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared" xmlns:t="urn:import:com.sap.sailing.gwt.common.client.controls.tabbar">
  <ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
  <ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
  <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.place.event2.header.EventHeaderResources" />
  <g:HTMLPanel addStyleNames="{res.mediaCss.grid}">
        <div class="{res.mediaCss.small12} {res.mediaCss.large9} {res.mediaCss.columns}">
          <div class="{local_res.css.eventheader_intro}">
            <div class="{local_res.css.eventheader_intro_logo}">
              <img ui:field="eventLogo" class="{local_res.css.eventheader_intro_logo_image}" />
            </div>
            <!-- IF header.dropdown === 'regattas' -->
            <div ui:field="dropdownTitle" class="{local_res.css.dropdown} {local_res.css.dropdownregattas} {local_res.css.jsdropdown}">
              <div class="{local_res.css.dropdown_head} {local_res.css.dropdownregattas_head} {local_res.css.jsdropdown_head}">
                <a ui:field="dropdownTrigger" class="{local_res.css.dropdown_head_title} {local_res.css.dropdownregattas_head_title}">
                  <span ui:field="dropdownEventName"></span>
                  <!-- IF event.state === 'live' -->
                  <div ui:field="dropdownEventState" data-labeltype="live" class="{res.mainCss.label} {res.mainCss.labellarge}">
                    <!-- <ui:text from='{i18n.live}' /> -->
                  </div>
                  <!-- IFEND event.state === 'live' -->
                  <div class="{local_res.css.dropdown_head_title_button} {local_res.css.dropdownregattas_head_title_button}"></div>
                </a>
              </div>
              <g:FlowPanel ui:field="dropdownContent" addStyleNames="{local_res.css.dropdown_content} {local_res.css.dropdownregattas_content} {local_res.css.jsdropdown_content}">
                <!-- EACH regattaItem in event.regattas -->
                <!-- TODO <s:eventHeader.RegattasDropdownItem ui:field='regattaItem'></s> -->
                  <!-- EACHEND -->
              </g:FlowPanel>
            </div>
            <!-- IFEND header.dropdown === 'regattas' -->
            <h2 ui:field="staticTitle" class="{local_res.css.eventheader_intro_name}">
              <span ui:field="eventName"></span>
              <!-- IF event.state === 'live' -->
              <div ui:field="eventState" data-labeltype="live" class="{res.mainCss.label}">
              </div>
              <!-- IFEND event.state === 'live' -->
            </h2>
            <div class="{local_res.css.eventheader_intro_details}">
              <!-- IF event.venue && event.eventType !== 'multi-regatta-event' -->
              <div ui:field="eventDate" class="{local_res.css.eventheader_intro_details_item}">
              </div>
              <div class="{local_res.css.eventheader_intro_details_item}">
                <img src="images/home/location-icon.png" class="{local_res.css.locationicon}"></img>
                <span ui:field="eventVenue"></span>
              </div>
              <a ui:field="eventLink" class="{local_res.css.eventheader_intro_details_item}"></a>
              <!-- IFEND event.venue && event.eventType !== 'multi-regatta-event' -->
              <!-- IF event.eventType === 'multi-regatta-event' -->
              <div ui:field="competitors" class="{local_res.css.eventheader_intro_details_item}">
                <span ui:field="competitorsCount"></span>
                <ui:text from='{i18n.competitors}' />
              </div>
              <div ui:field="races" class="{local_res.css.eventheader_intro_details_item}">
                <span ui:field="racesCount"></span>
                <ui:text from='{i18n.races}' />
              </div>
              <div ui:field="trackedRaces" class="{local_res.css.eventheader_intro_details_item}">
                <span ui:field="trackedRacesCount"></span>
                <ui:text from='{i18n.trackedRaces}' />
              </div>
              <div ui:field="eventCategory" class="{local_res.css.eventheader_intro_details_item}">
                <!-- event.category -->
              </div>
              <!-- IFEND event.eventType === 'multi-regatta-event' -->
            </div>
          </div>
        </div>
        <div class="{res.mediaCss.showonlarge} {res.mediaCss.large3} {res.mediaCss.columns}">
          <div class="{local_res.css.eventheader_sharing}">
            <a href="mailto:?&amp;subject=Lorem%20Ipsum%20Delorem&amp;body=Lorem%20ipsum%20dolor%20sit%20amet%2C%20consectetuer%20adipiscing%20elit.%20Aenean%20commodo%20ligula%20eget%20dolor.%20https%3A%2F%2Fstorage.ubidev.net%3Asap-sailing-analytics%3A6120578021%3Adev%3Aindex.html" title="undefined" class="{local_res.css.eventheader_sharing_item} {local_res.css.eventheader_sharing_itememail}"></a>
            <a href="https://twitter.com/intent/tweet?text=Lorem%20ipsum%20dolor%20sit%20amet%2C%20consectetuer%20adipiscing%20elit.%20Aenean%20commodo%20ligula%20eget%20dolor.&amp;url=https%3A%2F%2Fwww.ubilabs.net&amp;short_url_length=8" title="undefined" target="_blank" class="{local_res.css.eventheader_sharing_item} {local_res.css.eventheader_sharing_itemtwitter}"></a>
            <a href="https://www.facebook.com/dialog/feed?app_id=145634995501895&amp;display=page&amp;caption=Lorem%20ipsum%20dolor%20sit%20amet%2C%20consectetuer%20adipiscing%20elit.%20Aenean%20commodo%20ligula%20eget%20dolor.&amp;link=https%3A%2F%2Fstorage.ubidev.net%3Asap-sailing-analytics%3A6120578021%3Adev%3Aindex.html&amp;redirect_uri=https%3A%2F%2Fstorage.ubidev.net%3Asap-sailing-analytics%3A6120578021%3Adev%3Aindex.html" title="undefined" target="_blank" class="{local_res.css.eventheader_sharing_item} {local_res.css.eventheader_sharing_itemfacebook}"></a>
          </div>
        </div>
  </g:HTMLPanel>
</ui:UiBinder>