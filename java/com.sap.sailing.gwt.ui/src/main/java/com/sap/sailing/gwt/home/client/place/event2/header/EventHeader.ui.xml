            </div>
            <h2 class="{local_res.css.eventheader_intro_name}">
              Special Event<!-- TODO event.name -->
              <!-- IF event.state === 'live' -->
              <div data-labeltype="live" class="{res.mainCss.label}">
                live<!-- <ui:text from='{i18n.eventHeader.event.live}' /> -->
              </div>
              <!-- IFEND event.state === 'live' -->
            </h2>
            <div class="{local_res.css.eventheader_intro_details}">
