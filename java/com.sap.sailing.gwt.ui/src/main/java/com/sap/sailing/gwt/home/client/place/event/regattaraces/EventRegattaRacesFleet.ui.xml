<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
    <ui:with field="i18n" type="com.sap.sailing.gwt.home.client.i18n.TextMessages"/>
    <ui:with field="res" type="com.sap.sailing.gwt.home.client.HomeResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.shared.eventregattaraces.EventRegattaRacesFleetResources" />
    <g:HTMLPanel>

<div data-fleetcolor='{"r":"{fleet.color.r}","g":"{fleet.color.g}","b":"{fleet.color.b}"}' class="{local_res.css.eventregattaraces_phase_fleet}">
  <div style="background-color: [object Object]" class="{local_res.css.eventregattaraces_phase_fleet_count}"></div>
  <div class="{local_res.css.eventregattaraces_phase_fleet_name}">{fleet.name}</div>
</div>
    </g:HTMLPanel>
</ui:UiBinder>
