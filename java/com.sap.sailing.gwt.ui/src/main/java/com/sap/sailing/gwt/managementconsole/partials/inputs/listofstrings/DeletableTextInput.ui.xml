<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:sse="urn:import:com.sap.sse.gwt.client">
	<ui:with field="local_res" type="com.sap.sailing.gwt.managementconsole.partials.inputs.listofstrings.ListOfStringsInputResources" />
    <ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
    <ui:with field="app_res" type="com.sap.sailing.gwt.managementconsole.resources.ManagementConsoleResources" />
    
    <g:HTMLPanel ui:field="panel">
		<div class="{res.mediaCss.small10} {local_res.style.textInputWrapper}">
            <input ui:field="input" class="{local_res.style.textInput}"/>
        </div>
        <div ui:field="deleteWrapper" class="{res.mediaCss.small2} {local_res.style.deleteWrapper}">  
            <g:Anchor ui:field="delete" addStyleNames="{app_res.icons.icon} {app_res.icons.iconClose} {app_res.style.flexItemFixedWidth}" />
        </div>   
    </g:HTMLPanel>
</ui:UiBinder>
