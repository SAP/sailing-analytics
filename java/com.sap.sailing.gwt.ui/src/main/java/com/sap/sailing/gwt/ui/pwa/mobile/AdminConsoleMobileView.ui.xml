<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:mobile="urn:import:com.sap.sailing.gwt.ui.pwa.mobile"
    xmlns:sse="urn:import:com.sap.sse.gwt.client">
    <ui:with field="res"
        type="com.sap.sailing.gwt.common.client.SharedResources" />
    <ui:with field="local_res"
        type="com.sap.sailing.gwt.ui.pwa.mobile.AdminConsoleMobileResources" />
    <ui:with field="i18n"
        type="com.sap.sailing.gwt.ui.client.StringMessages" />
    <ui:style>
    .mainContent {
        height: 100%;
    }
    </ui:style>

    <g:HTMLPanel addStyleNames="{style.mainContent}">
        <mobile:header.Header ui:field="header" />
        <g:SimplePanel ui:field="content" />
        <div class="{res.mediaCss.grid}">
            <div
                class="{res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.column}">
                <h1>
                    Example Page
                </h1>
            </div>
            <div
                class="{res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.column}">
                <p>
                    Based on new UI design.
                </p>
            </div>
        </div>
        <div class="{res.mediaCss.grid}">
            <div
                class="{res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.column}">
                <label for="username">
                    USERNAME
                </label>
                <input id="username" placeholder="Username" />
            </div>
            <div
                class="{res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.column}">
                <label for="password">
                    PASSWORD
                </label>
                <input id="password" placeholder="Password"
                    type="password" />
            </div>
        </div>
        <div class="{res.mediaCss.grid}">
            <div
                class="{res.mediaCss.small12} {res.mediaCss.mediumoffset4} {res.mediaCss.medium4} {res.mediaCss.largeoffset6} {res.mediaCss.large3} {res.mediaCss.column}">
                <g:Button ui:field="settingsButton"
                    text="{i18n.releaseNotes}"
                    styleName="{local_res.css.primaryButton}" />
            </div>
            <div
                class="{res.mediaCss.small12} {res.mediaCss.medium4} {res.mediaCss.large3} {res.mediaCss.column}">
                <g:Button
                    ui:field="createRegattaLeaderboardButton"
                    text="{i18n.ok}"
                    styleName="{local_res.css.secondaryButton}" />
            </div>
            <div
                class="{res.mediaCss.small12} {res.mediaCss.column}">
                <div class="{local_res.css.secondaryCtaButton}">
                    <g:Button ui:field="forgotPasswordButton"
                        text="{i18n.forgotPassword}" />
                </div>
            </div>
        </div>
        <div class="{res.mediaCss.grid}">
            <div
                class="{res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.large4} {res.mediaCss.column}">
                <div class="{local_res.css.eventCard}">
                    <div class="{res.mediaCss.grid} {local_res.css.eventCardContainer}">
                        <div
                            class="{res.mediaCss.hideonsmall} {res.mediaCss.medium4} {res.mediaCss.column}">
                            <div
                                class="{local_res.css.featuredMarker}">
                                Featured
                            </div>
                        </div>
                        <h1
                            class="{res.mediaCss.small8} {res.mediaCss.medium12} {res.mediaCss.column}">
                            LYC Mittwochsregatta YS 2020
                        </h1>
                        <div
                            class="{res.mediaCss.small4} {res.mediaCss.showonsmall} {res.mediaCss.column}">
                            <div
                                class="{local_res.css.featuredMarker}">
                                Featured
                            </div>
                        </div>
                        <h2
                            class="{local_res.css.details} {local_res.css.location} {res.mediaCss.small12} {res.mediaCss.column}">
                            Travem端nde, May 6 - October 31, 2020
                        </h2>
                    </div>
                </div>
            </div>
            <div
                class="{res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.large4} {res.mediaCss.column}">
                <div class="{local_res.css.eventCard}">
                    <div class="{res.mediaCss.grid} {local_res.css.eventCardContainer}">
                        <div
                            class="{res.mediaCss.hideonsmall} {res.mediaCss.medium4} {res.mediaCss.column}">
                            <div
                                class="{local_res.css.featuredMarker}">
                                Featured
                            </div>
                        </div>
                        <h1
                            class="{res.mediaCss.small8} {res.mediaCss.medium12} {res.mediaCss.column}">
                            LYC Mittwochsregatta YS 2020
                        </h1>
                        <div
                            class="{res.mediaCss.small4} {res.mediaCss.showonsmall} {res.mediaCss.column}">
                            <div
                                class="{local_res.css.featuredMarker}">
                                Featured
                            </div>
                        </div>
                        <h2
                            class="{local_res.css.details} {local_res.css.location} {res.mediaCss.small12} {res.mediaCss.column}">
                            Travem端nde, May 6 - October 31, 2020
                        </h2>
                    </div>
                </div>
            </div>
            <div
                class="{res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.large4} {res.mediaCss.column}">
                <div class="{local_res.css.eventCard}">
                    <div class="{res.mediaCss.grid} {local_res.css.eventCardContainer}">
                        <div
                            class="{res.mediaCss.hideonsmall} {res.mediaCss.medium4} {res.mediaCss.column}">
                            <div
                                class="{local_res.css.featuredMarker}">
                                Featured
                            </div>
                        </div>
                        <h1
                            class="{res.mediaCss.small8} {res.mediaCss.medium12} {res.mediaCss.column}">
                            LYC Mittwochsregatta YS 2020
                        </h1>
                        <div
                            class="{res.mediaCss.small4} {res.mediaCss.showonsmall} {res.mediaCss.column}">
                            <div
                                class="{local_res.css.featuredMarker}">
                                Featured
                            </div>
                        </div>
                        <h2
                            class="{local_res.css.details} {local_res.css.location} {res.mediaCss.small12} {res.mediaCss.column}">
                            Travem端nde, May 6 - October 31, 2020
                        </h2>
                    </div>
                </div>
            </div>
            <div
                class="{res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.large4} {res.mediaCss.column}">
                <div class="{local_res.css.eventCard}">
                    <div class="{res.mediaCss.grid} {local_res.css.eventCardContainer}">
                        <div
                            class="{res.mediaCss.hideonsmall} {res.mediaCss.medium4} {res.mediaCss.column}">
                            <div
                                class="{local_res.css.featuredMarker}">
                                Featured
                            </div>
                        </div>
                        <h1
                            class="{res.mediaCss.small8} {res.mediaCss.medium12} {res.mediaCss.column}">
                            LYC Mittwochsregatta YS 2020
                        </h1>
                        <div
                            class="{res.mediaCss.small4} {res.mediaCss.showonsmall} {res.mediaCss.column}">
                            <div
                                class="{local_res.css.featuredMarker}">
                                Featured
                            </div>
                        </div>
                        <h2
                            class="{local_res.css.details} {local_res.css.location} {res.mediaCss.small12} {res.mediaCss.column}">
                            Travem端nde, May 6 - October 31, 2020
                        </h2>
                    </div>
                </div>
            </div>
        </div>
        <mobile:footer.Footer ui:field="footer" />
    </g:HTMLPanel>
</ui:UiBinder>