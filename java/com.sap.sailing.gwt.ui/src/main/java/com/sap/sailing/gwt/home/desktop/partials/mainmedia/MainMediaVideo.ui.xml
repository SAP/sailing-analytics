<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
	<ui:with field="local_res" type="com.sap.sailing.gwt.home.desktop.partials.mainmedia.MainMediaResources" />
	<g:HTMLPanel>
		<div class="{res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.large4} {res.mediaCss.columns}">
			<figure class="{local_res.css.videopreview}">
				<g:SimplePanel styleName="{local_res.css.videopreview_videocontainer}" ui:field="videoHolderUi" />
				<figcaption ui:field="videoTitleWrapper">
					<span ui:field="videoTitle" class="{local_res.css.videopreview_title}" />
				</figcaption>
			</figure>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
