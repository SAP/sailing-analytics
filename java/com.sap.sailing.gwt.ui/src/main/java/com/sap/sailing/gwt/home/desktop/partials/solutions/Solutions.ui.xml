<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:sb="urn:import:com.sap.sailing.gwt.home.shared.partials.storebadges">
  <ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources"/>
  <ui:with field="local_res" type="com.sap.sailing.gwt.home.desktop.partials.solutions.SolutionsResources"/>

  <g:HTMLPanel>
    <div class="{local_res.css.solutions}">

      <div class="{local_res.css.solutions_nav}">
        <div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
          <div class="{res.mediaCss.small12} {res.mediaCss.columns}">
            <g:Anchor ui:field="solution1Anchor" styleName="{local_res.css.solutions_nav_link}"/>
            <g:Anchor ui:field="solution2Anchor" styleName="{local_res.css.solutions_nav_link}"/>
            <g:Anchor ui:field="solution3Anchor" styleName="{local_res.css.solutions_nav_link}"/>
            <g:Anchor ui:field="solution4Anchor" styleName="{local_res.css.solutions_nav_link}"/>
            <g:Anchor ui:field="solution5Anchor" styleName="{local_res.css.solutions_nav_link}"/>
            <g:Anchor ui:field="solution6Anchor" styleName="{local_res.css.solutions_nav_link}"/>
          </div>
        </div>
      </div>

      <div class="{local_res.css.solutions}">

        <div ui:field="solution1Div" class="{local_res.css.solutions_content} {local_res.css.solutions_contentsapinsailing}">
          <div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
            <div class="{res.mediaCss.small12} {res.mediaCss.columns}">
              <div ui:field="solution1Inner" class="{local_res.css.solutions_contentsapinsailing_body}">
                <h2 ui:field="solution1Title"/>
                <p ui:field="solution1P1"/>
                <ul ui:field="solution1List1"/>
                <p ui:field="solution1P2"/>
                <ul ui:field="solution1List2"/>
              </div>
            </div>
          </div>
        </div>
        <div ui:field="solution2Div" class="{local_res.css.solutions_content} {local_res.css.solutions_contentsap}">
          <div ui:field="solution2Bg" class="{res.mediaCss.grid} {local_res.css.gridalternator}">
            <div class="{res.mediaCss.small5} {res.mediaCss.smalloffset7} {res.mediaCss.columns}">
              <h2 ui:field="solution2Title"/>
              <p ui:field="solution2P1"/>
              <p ui:field="solution2P2"/>
              <g:Anchor ui:field="solution2ReadMore" styleName="{res.mainCss.button}"/>
            </div>
          </div>
        </div>

        <div ui:field="solution3Div" class="{local_res.css.solutions_content} {local_res.css.solutions_contentsapsailingracemanager}">
          <div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
            <div class="{res.mediaCss.small12} {res.mediaCss.column}">
              <div ui:field="solution3Bg" class="{local_res.css.solutions_contentsapsailingracemanager_body}">
                <h2 ui:field="solution3Title"/>
                <p ui:field="solution3P"/>
                <sb:PlayStoreBadge ui:field="solution3PlayBadge" targetUrl=""/>
                <g:Anchor ui:field="solution3ReadMore" styleName="{res.mainCss.button}"/>
              </div>
            </div>
          </div>
        </div>

        <div ui:field="solution4Div" class="{local_res.css.solutions_content} {local_res.css.solutions_contentsapsailinsight}">
          <div ui:field="solution4Bg" class="{res.mediaCss.grid} {local_res.css.gridalternator}">
            <div class="{res.mediaCss.small5} {res.mediaCss.smalloffset7} {res.mediaCss.columns}">
              <h2 ui:field="solution4Title"/>
              <p ui:field="solution4P"/>
              <sb:AppStoreBadge  ui:field="solution4AppBadge" targetUrl=""/>
              <sb:PlayStoreBadge ui:field="solution4PlayBadge" targetUrl=""/>
              <g:Anchor ui:field="solution4ReadMore" styleName="{res.mainCss.button}"/>
            </div>
          </div>
        </div>

        <div ui:field="solution5Div" class="{local_res.css.solutions_content} {local_res.css.solutions_contentsapsailingbuoypinger}">
          <div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
            <div class="{res.mediaCss.small12} {res.mediaCss.column}">
              <div ui:field="solution5Bg" class="{local_res.css.solutions_contentsapsailingbuoypinger_body}">
                <h2 ui:field="solution5Title"/>
                <p ui:field="solution5P"/>
                <sb:PlayStoreBadge ui:field="solution5PlayBadge" targetUrl=""/>
                <g:Anchor ui:field="solution5ReadMore" styleName="{res.mainCss.button}"/>
              </div>
            </div>
          </div>
        </div>

        <div ui:field="solution6Div" class="{local_res.css.solutions_content} {local_res.css.solutions_contentsimulator}">
          <div ui:field="solution6Bg" class="{res.mediaCss.grid} {local_res.css.gridalternator}">
            <div class="{res.mediaCss.small5} {res.mediaCss.smalloffset7} {res.mediaCss.columns}">
              <h2 ui:field="solution6Title"/>
              <p ui:field="solution6P"/>
              <g:Anchor ui:field="solution6ReadMore" styleName="{res.mainCss.button}"/>
            </div>
          </div>
        </div>

      </div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>