<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
	<ui:with field="i18n"
		type="com.sap.sailing.gwt.common.client.i18n.TextMessages" />
	<ui:with field="res"
		type="com.sap.sailing.gwt.common.client.SharedResources" />
	<ui:with field="local_res"
		type="com.sap.sailing.gwt.home.desktop.partials.solutions.SolutionsResources" />
	<g:HTMLPanel>
		<!-- Page displays all solutions -->
		<div class="{local_res.css.solutions}">
			<div class="{local_res.css.solutions_nav}">
				<div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
					<div class="{res.mediaCss.small12} {res.mediaCss.columns}">
						<g:Anchor ui:field="sapInSailingAnchor" styleName="{local_res.css.solutions_nav_link}">
							<ui:text from='{i18n.solutionsSISHeadline}' />
						</g:Anchor>
						<g:Anchor ui:field="sailingAnalyticsAnchor" styleName="{local_res.css.solutions_nav_link}">
							<ui:text from='{i18n.solutionsSAPHeadline}' />
						</g:Anchor>
						<g:Anchor ui:field="raceAnchor" styleName="{local_res.css.solutions_nav_link}">
							<ui:text from='{i18n.solutionsRaceHeadline}' />
						</g:Anchor>
						<g:Anchor ui:field="postRaceAnchor" styleName="{local_res.css.solutions_nav_link}">
							<ui:text from='{i18n.solutionsPostHeadline}' />
						</g:Anchor>
						<g:Anchor ui:field="trainingDiaryAnchor" styleName="{local_res.css.solutions_nav_link}">
							<ui:text from='{i18n.solutionsTrainingHeadline}' />
						</g:Anchor>
						<g:Anchor ui:field="simulatorAnchor" styleName="{local_res.css.solutions_nav_link}">
							<ui:text from='{i18n.solutionsSimulatorHeadline}' />
						</g:Anchor>
					</div>
				</div>
			</div>


			<div class="{local_res.css.solutions}">
				<div ui:field="sapInSailingDiv"
					class="{local_res.css.solutions_content} {local_res.css.solutions_contentsapinsailing}">
					<div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
						<div class="{res.mediaCss.small12} {res.mediaCss.columns}">
							<div class="{local_res.css.solutions_contentsapinsailing_body}">
								<h2>
									<ui:text from='{i18n.sapInSailing}' />
								</h2>
								<p>
									<ui:text from='{i18n.contentSapInSailing1}' />
								</p>
								<ul>
									<li>
										<ui:text from='{i18n.contentSapInSailing2}' />
									</li>
									<li>
										<ui:text from='{i18n.contentSapInSailing3}' />
									</li>
									<li>
										<ui:text from='{i18n.contentSapInSailing4}' />
									</li>
								</ul>
								<p>
									<ui:text from='{i18n.contentSapInSailing5}' />
								</p>
								<ul>
									<li>
										<ui:text from='{i18n.contentSapInSailing6}' />
									</li>
									<li>
										<ui:text from='{i18n.contentSapInSailing7}' />
									</li>
									<li>
										<ui:text from='{i18n.contentSapInSailing8}' />
									</li>
									<li>
										<ui:text from='{i18n.contentSapInSailing9}' />
									</li>
									<li>
										<ui:text from='{i18n.contentSapInSailing10}' />
									</li>
									<li>
										<ui:text from='{i18n.contentSapInSailing11}' />
									</li>
									<li>
										<ui:text from='{i18n.contentSapInSailing12}' />
									</li>
								</ul>
							</div>
						</div>
					</div>
				</div>
				<div ui:field="sailingAnalyticsDiv"
					class="{local_res.css.solutions_content} {local_res.css.solutions_contentsap}">
					<div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
						<div
							class="{res.mediaCss.small5} {res.mediaCss.smalloffset7} {res.mediaCss.columns}">
							<h2>
								<ui:text from='{i18n.sapSailingAnalytics}' />
							</h2>
							<p>
								<ui:text from='{i18n.contentSapSailingAnalytics1}' />
							</p>
							<p>
								<ui:text from='{i18n.contentSapSailingAnalytics2}' />
							</p>
							<g:Anchor ui:field="sailingAnalyticsDetailsAnchor"
								styleName="{res.mainCss.button}">
								<ui:text from='{i18n.sapSailingAnalyticsReadMore}' />
							</g:Anchor>
						</div>
					</div>
				</div>
				<div ui:field="raceDiv"
					class="{local_res.css.solutions_content} {local_res.css.solutions_contentsapsailingracemanager}">
					<div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
						<div class="{res.mediaCss.small12} {res.mediaCss.column}">
							<div
								class="{local_res.css.solutions_contentsapsailingracemanager_body}">
								<h2>
									<ui:text from='{i18n.sapSailingRaceManager}' />
								</h2>
								<p>
									<ui:text from='{i18n.contentSapSailingRaceManager}' />
								</p>
								<g:Anchor ui:field="raceCommitteeAppDetailsAnchor"
									styleName="{res.mainCss.button}">
									<ui:text from='{i18n.sapSailingRaceManagerReadMore}' />
								</g:Anchor>
							</div>
						</div>
					</div>
				</div>
				<div ui:field="postRaceDiv"
					class="{local_res.css.solutions_content} {local_res.css.solutions_contentpost}">
					<div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
						<div
							class="{res.mediaCss.small5} {res.mediaCss.smalloffset7} {res.mediaCss.columns}">
							<h2>
								<ui:text from='{i18n.postRaceAnalytics}' />
							</h2>
							<p>
								<ui:text from='{i18n.contentPostRaceAnalytics1}' />
							</p>
							<p>
								<ui:text from='{i18n.contentPostRaceAnalytics2}' />
							</p>
						</div>
					</div>
				</div>
				<div ui:field="trainingDiaryDiv"
					class="{local_res.css.solutions_content} {local_res.css.solutions_contenttraining}">
					<div class="{local_res.css.parallax}">
						<div class="{local_res.css.background}"></div>
					</div>
					<div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
						<div class="{res.mediaCss.small5} {res.mediaCss.columns}">
							<h2>
								<ui:text from='{i18n.i18nDeSolutionsTrainingHeadline}' />
							</h2>
							<p>
								<ui:text from='{i18n.i18nDeSolutionsTrainingText}' />
							</p>
							<a
								href="https://itunes.apple.com/de/app/stg-trainingstagebuch/id779563635?l=en&amp;mt=8"
								target="_blank" class="{local_res.css.solutions_content_linkappstore}">
								<img src="images/home/appstore.png"></img>
							</a>
						</div>
					</div>
				</div>
				<div ui:field="simulatorDiv"
					class="{local_res.css.solutions_content} {local_res.css.solutions_contentsimulator}">
					<div class="{res.mediaCss.grid} {local_res.css.gridalternator}">
						<div
							class="{res.mediaCss.small5} {res.mediaCss.smalloffset7} {res.mediaCss.columns}">
							<h2>
								<ui:text from='{i18n.strategySimulator}' />
							</h2>
							<p>
								<ui:text from='{i18n.contentStrategySimulator}' />
							</p>
							<g:Anchor ui:field="simulatorAppDetailsAnchor"
								styleName="{res.mainCss.button}">
								<ui:text from='{i18n.strategySimulatorReadMore}' />
							</g:Anchor>
						</div>
					</div>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
