<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style gss="true">
		@external slick-active;
		.mainSlider {
			height: 100%;
		}
		.mainSlider div {
			height: 100%;
		}
		.content {
			width: 100%;
			height: 100%;
		}
	</ui:style>
	<g:SimpleLayoutPanel>
		<g:HTMLPanel styleName="{style.content}">
			<div class="mainSlider {style.mainSlider}" ui:field="mainSliderUi"></div>
		</g:HTMLPanel>
	</g:SimpleLayoutPanel>
</ui:UiBinder> 