<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:s="urn:import:com.sap.sailing.gwt.home.client.shared">
    <ui:with field="i18n" type="com.sap.sailing.gwt.home.client.i18n.TextMessages"/>
    <ui:with field="res" type="com.sap.sailing.gwt.home.client.HomeResources" />
    <ui:with field="local_res" type="com.sap.sailing.gwt.home.client.shared.mainmedia.MainMediaResources" />
    <g:HTMLPanel>
<div class="{res.mainCss.mainsection} {local_res.css.media}">
  <div class="{res.mainCss.grid}">
    <div class="{res.mediaCss.small12} {res.mediaCss.columns}">
      <header class="{res.mainCss.mainsection_header}">
        <h3 class="{res.mainCss.mainsection_header_title}">Sailing loves Photography</h3>
        <p>Get lost in the latest and most beautiful impressions from your favorite sailing event.</p>
      </header>
    </div>
  </div>
  <div class="{res.mainCss.grid}">
    <div class="{res.mediaCss.small12} {res.mediaCss.columns}">
      <div class="{local_res.css.media_slideshow}">
        <div style="background-image: url('images/xyz.png')" class="{local_res.css.media_photo}"></div>
        <div class="{local_res.css.media_slideshow_controls} {local_res.css.media_slideshow_controlsprev}"></div>
        <div class="{local_res.css.media_slideshow_controls} {local_res.css.media_slideshow_controlsnext}"></div>
        <div class="{local_res.css.media_slideshow_controls} {local_res.css.media_slideshow_controlsfullsize}"></div>
      </div>
    </div>
  </div>
</div>
<div class="{res.mainCss.mainsection} {local_res.css.media} {local_res.css.mediavideos}">
  <div class="{res.mainCss.grid}">
    <div class="{res.mediaCss.small12} {res.mediaCss.columns}">
      <header class="{res.mainCss.mainsection_header}">
        <h3 class="{res.mainCss.mainsection_header_title}">Recent Video Entries</h3>
        <p>Immerse yourself in the world of sailing and check out our latest events for more videos.</p>
      </header>
    </div>
  </div>
  <div class="{res.mainCss.grid}">
    <div class="{res.mediaCss.small12} {res.mediaCss.medium6} {res.mediaCss.large4} {res.mediaCss.columns}">
      <figure class="{local_res.css.videopreview}"><img src="images/video.preview" />
        <figcaption>
          <p class="{local_res.css.videopreview_title}"><g:Label ui:field="videoTitle" /></p>
          <div><g:Label ui:field="videoLength" /></div>
        </figcaption>
      </figure>
    </div>
  </div>
  <div class="{res.mainCss.grid} {res.mainCss.mainsection_navigation}">
    <div class="{res.mediaCss.small12} {res.mediaCss.columns}"><a href="#" class="{res.mainCss.button}">Show More</a></div>
  </div>
</div>
    </g:HTMLPanel>
</ui:UiBinder>
