<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:e="urn:import:com.sap.sailing.gwt.home.shared.partials.editable">
    <ui:with field="res" type="com.sap.sailing.gwt.home.shared.partials.editable.EditableResources" />
    <g:HTMLPanel>
        <g:TextBox ui:field="textBoxUi" styleName="{res.css.inlineElement}" height="45" width="45" />
        <g:Label ui:field="labelUi" styleName="{res.css.inlineEditLabel} {res.css.inlineElement}" />
        <e:InlineEditButton ui:field="editButtonUi" styleName="{res.css.inlineElement}" />
    </g:HTMLPanel>
</ui:UiBinder> 