<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="i18n" type="com.sap.sailing.gwt.ui.client.StringMessages" />
	<ui:with field="res" type="com.sap.sailing.gwt.common.client.SharedResources" />
	<ui:with field="local_res" type="com.sap.sailing.gwt.home.desktop.partials.databylogo.DataByLogoResources" />
    <g:HTMLPanel addStyleNames="{local_res.css.eventheader_intro_details_item} {local_res.css.eventheader_dataBy_logo_container}" debugId="EventHeaderPanel">
	    <a ui:field="dataByContainer" target="_blank" href="" class="{local_res.css.eventheader_dataBy_subtleLink}">
            <ui:text from="{i18n.dataBy}"/>
            <g:Image ui:field="logo" addStyleNames="{local_res.css.eventheader_dataBy_logo}"></g:Image>
        </a>
    </g:HTMLPanel>
</ui:UiBinder> 