<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with field="i18n" type="com.sap.sse.security.ui.client.i18n.StringMessages" />
    <ui:with field="adr"
        type="com.sap.sse.security.ui.client.component.editacl.AclDialogResources" />
    <g:HTMLPanel>
        <div class= "{adr.css.dialogHeader}">
            <g:Label text="{i18n.id}:"/>
            <g:Label text="{i18n.permissionType}:"/>
        </div>
        <div style="display:inline-block;">
            <g:Label ui:field="lblId"/>
            <g:Label ui:field="lblType"/>
        </div>
        <g:FlowPanel ui:field="userGroupCellListPanelUi" />
        <g:FlowPanel ui:field="permissionsCellListPanelUi" />
    </g:HTMLPanel>
</ui:UiBinder> 