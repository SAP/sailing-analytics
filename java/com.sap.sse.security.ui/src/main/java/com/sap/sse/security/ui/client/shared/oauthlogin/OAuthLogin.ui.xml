<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with field="i18n" type="com.sap.sse.security.ui.client.i18n.StringMessages"/>
    <ui:with field="local_res" type="com.sap.sse.security.ui.client.shared.oauthlogin.OAuthLoginResources" />
	<g:HTMLPanel>
	    <div class="{local_res.css.oauthPanel}">
	        <g:Anchor href="javascript:;" ui:field="facebookAnchor">
	        	<img src="{local_res.facebookIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="facebook" />
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="googleAnchor">
	        	<img src="{local_res.googleIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="Google"/>
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="twitterAnchor">
	        	<img src="{local_res.twitterIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="Twitter" />
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="linkedinAnchor">
	        	<img src="{local_res.linkedinIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="LinkedIn" />
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="yahooAnchor">
	        	<img src="{local_res.yahooIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="Yahoo" />
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="vimeoAnchor">
	        	<img src="{local_res.vimeoIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="Vimeo" />
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="githubAnchor">
	        	<img src="{local_res.githubIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="github" />
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="instagramAnchor">
	        	<img src="{local_res.instagramIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="Instagram" />
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="flickrAnchor">
	        	<img src="{local_res.flickrIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="flickr" />
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="liveAnchor">
	        	<img src="{local_res.liveIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="Windows Live" />
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="tumblrAnchor">
	        	<img src="{local_res.tumblrIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="tumblr." />
	        </g:Anchor>
	        <g:Anchor href="javascript:;" ui:field="foursquareAnchor">
	        	<img src="{local_res.foursquareIcon.getSafeUri}" class="{local_res.css.oauthLogo}" title="foursquare" />
	        </g:Anchor>
	    </div>
	</g:HTMLPanel>     
</ui:UiBinder>    
