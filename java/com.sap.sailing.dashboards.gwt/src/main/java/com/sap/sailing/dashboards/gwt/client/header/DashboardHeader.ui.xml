<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:widget="urn:import:com.sap.sailing.dashboards.gwt.client.widgets">

	<ui:with field="resources" type="com.sap.sailing.dashboards.gwt.client.header.DashboardHeaderResources"/>

	<g:HTMLPanel>
		<g:Image ui:field="sapLogo" styleName="{resources.gss.sap_logo}"></g:Image>
		<div class="{resources.gss.event_race_container}">
			<div ui:field="event" class="{resources.gss.event}"></div>
			<div ui:field="race" class="{resources.gss.race}"></div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 