<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:highcharts="urn:import:org.moxieapps.gwt.highcharts.client">

	<ui:with field="resources" type="com.sap.sailing.dashboards.gwt.client.windchart.VerticalWindChartResources"/>

	<g:FlowPanel styleName="{resources.gss.verical_wind_chart}">
		<highcharts:Chart ui:field="verticalWindChart" styleName="{resources.gss.chart}"></highcharts:Chart>
	<g:FocusPanel ui:field="verticalWindChartClickArea" styleName="{resources.gss.click_area}"></g:FocusPanel>
	</g:FlowPanel>
</ui:UiBinder> 