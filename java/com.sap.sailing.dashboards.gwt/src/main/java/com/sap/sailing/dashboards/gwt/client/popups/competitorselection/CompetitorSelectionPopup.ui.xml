<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:table="urn:import:com.sap.sailing.dashboards.gwt.client.popups.competitorselection.table"
	xmlns:mgwts="urn:import:com.googlecode.mgwt.ui.client.widget.panel.scroll"
	xmlns:action="urn:import:com.sap.sailing.dashboards.gwt.client.widgets">

	<ui:with field="resources" type="com.sap.sailing.dashboards.gwt.client.popups.competitorselection.CompetitorSelectionPopupResources" />

	<g:HTMLPanel>
		<action:ActionPanel ui:field="competitorselectionbackground" styleName="{resources.gss.competitorselectionbackground}"></action:ActionPanel>
			<g:HTMLPanel ui:field="competitorselectionpopup" styleName="{resources.gss.competitorselectionpopup}">
				<g:HTMLPanel ui:field="header" styleName="{resources.gss.header}">Select your Team</g:HTMLPanel>
				<mgwts:ScrollPanel styleName="{resources.gss.scrollpanel}" scrollingEnabledX="false">
					<g:FlowPanel>
						<table:CompetitorTable ui:field="competitortable"></table:CompetitorTable>
					</g:FlowPanel>
				</mgwts:ScrollPanel>
				<action:ActionPanel ui:field="cancelButton" styleName="{resources.gss.button} {resources.gss.cancelButton}"></action:ActionPanel>
				<div class="{resources.gss.buttonSeparator}"></div>
				<action:ActionPanel ui:field="okButton" styleName="{resources.gss.button} {resources.gss.okButton}"></action:ActionPanel>
			</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder> 