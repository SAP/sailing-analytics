<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style
		type='com.sap.sailing.dashboards.gwt.client.startanalysis.StartlineAnalysisCard.StartlineAnalysis'
		src='StartlineAnalysis.css'></ui:style>

	<g:HTMLPanel ui:field="startanalysis_card" styleName="{style.startanalysis_card}">
		<!-- <maps:MapWidget></maps:MapWidget> -->
		<div class="{style.startanalysis_card_wind_line_data_container}">
			<g:HTMLPanel ui:field="winddirection"
				styleName="{style.startanalysis_card_wind_direction_image}">
			</g:HTMLPanel>
			<div ui:field="startanalysis_card_wind_data_direction"
				class="{style.startanalysis_card_data} {style.startanalysis_card_wind_data_direction}"></div>
			<div ui:field="startanalysis_card_wind_data_speed"
				class="{style.startanalysis_card_data} {style.startanalysis_card_wind_data_speed}"></div>
			<div ui:field="startanalysis_card_line_advantage"
				class="{style.startanalysis_card_data} {style.startanalysis_card_line_advantage}"></div>
		</div>
		<g:HTMLPanel ui:field="startanalysis_card_table"
			styleName="{style.startanalysis_card_table}"></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 