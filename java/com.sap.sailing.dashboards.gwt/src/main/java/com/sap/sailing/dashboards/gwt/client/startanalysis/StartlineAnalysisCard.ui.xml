<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style
		type='com.sap.sailing.dashboards.gwt.client.startanalysis.StartlineAnalysisCard.StartlineAnalysis'
		src='StartlineAnalysis.css'></ui:style>

	<g:HTMLPanel ui:field="startanalysis_card" styleName="{style.startanalysis_card}">
		<div class="{style.startanalysis_card_wind_line_data_container}">
			<div ui:field="startanalysis_card_line_advantage"
				class="{style.startanalysis_card_data} {style.startanalysis_card_line_advantage}"></div>
		</div>
		<div ui:field="startanalysis_card_race_time"
				class="{style.startanalysis_card_race_time}">Time: 00:05:00</div>
		<g:HTMLPanel ui:field="startanalysis_card_table"
			styleName="{style.startanalysis_card_table}"></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 