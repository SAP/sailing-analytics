<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:util="urn:import:com.sap.sailing.dashboards.gwt.client.widgets.startlineadvantage.util"
	xmlns:chart="urn:import:com.sap.sailing.dashboards.gwt.client.widgets.startlineadvantage.wind.charts"
	xmlns:header="urn:import:com.sap.sailing.dashboards.gwt.client.widgets.header">

	<ui:with field="resources" type="com.sap.sailing.dashboards.gwt.client.widgets.startlineadvantage.wind.StartlineAdvantagesByWindWidgetResources" />

	<g:HTMLPanel styleName="{resources.gss.start_line_advantages_by_wind_component}">
		<header:DashboardWidgetHeaderAndNoDataMessage
			ui:field="dashboardWidgetHeaderAndNoDataMessage"></header:DashboardWidgetHeaderAndNoDataMessage>
		<g:HTMLPanel ui:field="dataDisplayContainer">
			<g:HTMLPanel styleName="{resources.gss.advantage_maximum_live_average}">
				<util:LiveAverageComponent ui:field="advantageMaximumLiveAverage"></util:LiveAverageComponent>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{resources.gss.advantages_on_line_chart_container}">
				<chart:StartlineAdvantagesOnLineChart ui:field="startlineAdvantagesOnLineChart"></chart:StartlineAdvantagesOnLineChart>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 