<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:util="urn:import:com.sap.sailing.dashboards.gwt.client.startlineadvantage.util"
	xmlns:chart="urn:import:com.sap.sailing.dashboards.gwt.client.startlineadvantage.wind.chart">

    <ui:with field="resources" type="com.sap.sailing.dashboards.gwt.client.startlineadvantage.wind.StartlineAdvantagesByWindComponentResources"/>

	<g:HTMLPanel styleName="{resources.gss.start_line_advantages_by_wind_component}">
		<g:HTMLPanel styleName="{resources.gss.advantage_maximum_live_average}">
			<util:LiveAverageComponent ui:field="advantageMaximumLiveAverage"></util:LiveAverageComponent>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{resources.gss.advantages_on_line_chart_container}">
			<chart:StartlineAdvantagesOnLineChart ui:field="startlineAdvantagesOnLineChart"></chart:StartlineAdvantagesOnLineChart>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 