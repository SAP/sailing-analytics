<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:highcharts="urn:import:org.moxieapps.gwt.highcharts.client">

    <ui:with field="resources" type="com.sap.sailing.dashboards.gwt.client.widgets.startlineadvantage.wind.charts.StartlineAdvantagesOnLineChartResources"/>

	<g:HTMLPanel>
	<g:HTMLPanel ui:field="chartContainer" styleName="{resources.gss.advantages_on_line_chart_container}"></g:HTMLPanel>
	<div class="{resources.gss.confidence_bar_container}">
			<div class="{resources.gss.confidence_bar}"></div>
			<div ui:field="confidenceBarHeader" class="{resources.gss.confidence_bar_header}"></div>
			<div ui:field="confidenceBarMinLabel" class="{resources.gss.confidence_bar_label} {resources.gss.confidence_bar_min_label}"></div>
			<div ui:field="confidenceBarMaxLabel" class="{resources.gss.confidence_bar_label} {resources.gss.confidence_bar_max_label}"></div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 