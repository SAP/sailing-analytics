	<g:Image ui:field="settingsButtonImage" styleName="{style.settings_button_image}"></g:Image>
	<g:Label ui:field="settingsIndicationLabel" styleName="{style.settings_indication_label}"></g:Label>
	</g:HTMLPanel>
</ui:UiBinder> 