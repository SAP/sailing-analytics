<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module>
   <source path='common'/>
   <!-- Avoid the "u2f" identifier to be used during obfuscation; it conflicts with
        the two-factor authentication library that browsers in 2019 have started to
        map to window.u2f. See also bug 5161.
     -->
   <extend-configuration-property name="js.identifier.blacklist" value="u2f"/>
</module>
