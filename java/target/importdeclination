#!/bin/sh
#!/bin/sh
target="`dirname \"$0\"`"
pwd=`pwd`
echo "$target" | grep "^/"
if [ "$0" != "0" ]; then
  # no leading slash, therefore no absolute path; prepend pwd
  target="$pwd/$target"
fi
echo target is $target
pluginsdir="$target/../com.sap.sailing.feature.p2build/bin/products/raceanalysis.product.id/win32/win32/x86_64/plugins"
mkdir resources
classpath="`find $pluginsdir -name 'com.sap.sailing.domain_*.jar' | sort | tail -1`:`find $pluginsdir -name 'com.sap.sailing.domain.common_*.jar' | sort | tail -1`:`find $pluginsdir -name 'com.sap.sailing.domain.shared.android_*.jar' | sort | tail -1`:`find $pluginsdir -name 'com.sap.sailing.declination*.jar' | sort | tail -1`"
echo Using classpath $classpath
"$JAVA_HOME/bin/java" -cp "$classpath" -Djava.util.logging.config.file=$target/configuration/logging.properties com.sap.sailing.declination.impl.DeclinationStore $*
